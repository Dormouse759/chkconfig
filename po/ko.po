#: ../chkconfig.c:23
msgid ""
"This may be freely redistributed under the terms of the GNU Public License.\n"
msgstr "이 프로그램은 GNU Public License하에서 자유롭게 재배포가 가능합니다.\n"

#: ../chkconfig.c:26
#, c-format
msgid "usage:   %s --list [name]\n"
msgstr "사용법:  %s --list [이름]\n"

#: ../chkconfig.c:27
#, c-format
msgid "         %s --add <name>\n"
msgstr "         %s --add <이름>\n"

#: ../chkconfig.c:28
#, c-format
msgid "         %s --del <name>\n"
msgstr "         %s --del <이름>\n"

#: ../chkconfig.c:29
#, c-format
msgid "         %s [--level <levels>] <name> %s)\n"
msgstr "         %s [--level <런레벨>] <이름> %s\n"

#: ../chkconfig.c:36
#, c-format
msgid "service %s does not support chkconfig\n"
msgstr "%s 서비스는 chkconfig을 지원하지 않음\n"

#: ../chkconfig.c:38
#, c-format
msgid "error reading information on service %s: %s\n"
msgstr "%s 서비스의 정보를 읽는 중 오류: %s\n"

#: ../chkconfig.c:100
msgid "on"
msgstr "켬"

#: ../chkconfig.c:100
msgid "off"
msgstr "끔"

#: ../chkconfig.c:117
#, c-format
msgid "failed to open %s/init.d: %s\n"
msgstr "%s/init.d를 여는데 실패했습니다: %s\n"

#: ../chkconfig.c:151
#, c-format
msgid "error reading from directory %s/init.d: %s"
msgstr "%s/init.d 디렉토리를 읽는 중 오류: %s"

#: ../chkconfig.c:231
#, c-format
msgid "%s version %s\n"
msgstr "%s 버젼 %s"

#: ../chkconfig.c:238
msgid "only one of --list, --add, or --del may be specified\n"
msgstr "--list, --add, --del 중에 하나만을 사용해야 함\n"

#: ../chkconfig.c:285
msgid "only one runlevel may be specfied for a chkconfig query\n"
msgstr ""

#: ../ntsysv.c:30
msgid "Press <F1> for more information on a service."
msgstr "<F1>을 누르면 서비스에 대한 자세한 정보를 보여줍니다."

#: ../ntsysv.c:56 ../ntsysv.c:93
msgid "Ok"
msgstr "확인"

#: ../ntsysv.c:56
msgid "Back"
msgstr "뒤로"

#: ../ntsysv.c:56
msgid "Cancel"
msgstr "취소"

#: ../ntsysv.c:68
msgid "What services should be automatically started?"
msgstr "어떤 서비스를 자동으로 시작할까요?"

#: ../ntsysv.c:74
msgid "Services"
msgstr "서비스"

#: ../ntsysv.c:164
#, c-format
msgid "error reading info for service %s: %s\n"
msgstr "%s 서비스의 정보를 읽는 중 오류: %s\n"

#: ../ntsysv.c:173
#, c-format
msgid "error reading from directory %s/init.d: %s\n"
msgstr "%s/init.d 디렉토리를 읽는 중 오류: %s\n"

#: ../ntsysv.c:219
msgid "bad argument to --levels\n"
msgstr "--levels에 잘못된 인자\n"

#: ../ntsysv.c:226
msgid "No services may be managed by ntsysv!\n"
msgstr "ntsysv로 관리할 수 있는 서비스가 없는 것 같군요!\n"

#: ../leveldb.c:265
#, c-format
msgid "failed to glob pattern %s: %s\n"
msgstr ""

#: ../leveldb.c:296
msgid "cannot determine current run level\n"
msgstr "현재 런레벨을 알아낼 수 없음\n"

#: ../leveldb.c:330
#, c-format
msgid "failed to make symlink %s: %s\n"
msgstr "%s symlink 생성 실패: %s\n"
