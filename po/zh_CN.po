#: ../chkconfig.c:50
#, c-format
msgid "usage:   %s [--list] [--type <type>] [name]\n"
msgstr "用法：   %s [--list] [--type <类型>] [名称]\n"

#: ../chkconfig.c:51
#, c-format
msgid "         %s --add <name>\n"
msgstr "         %s --add <名称>\n"

#: ../chkconfig.c:52
#, c-format
msgid "         %s --del <name>\n"
msgstr "         %s --del <名称>\n"

#: ../chkconfig.c:53
#, c-format
msgid "         %s --override <name>\n"
msgstr "         %s --override <名称>\n"

#: ../chkconfig.c:54
#, c-format
msgid "         %s [--level <levels>] [--type <type>] <name> %s\n"
msgstr "         %s [--level <级别>] [--type <类型>] <名称> %s\n"

#: ../chkconfig.c:61
#, c-format
msgid ""
"service %s supports chkconfig, but is not referenced in any runlevel (run "
"'chkconfig --add %s')\n"
msgstr "服务 %s 支持 chkconfig，但它未在任何运行级别中引用（请运行“chkconfig --add %s”添加引用）\n"

