#: ../chkconfig.c:21
#, c-format
msgid "%s version %s - Copyright (C) 1997-2000 Red Hat, Inc.\n"
msgstr "%s útgáfa %s - Höfundarréttur (C) 1997-2000 Red Hat HF.\n"

#: ../chkconfig.c:22
msgid ""
"This may be freely redistributed under the terms of the GNU Public License.\n"
msgstr "Þessu má dreifa samkvæmt skilmálum GNU Public License.\n"

#: ../chkconfig.c:25
#, c-format
msgid "usage:   %s --list [name]\n"
msgstr "notkun:   %s --list [nafn]\n"

#: ../chkconfig.c:26
#, c-format
msgid "         %s --add <name>\n"
msgstr "         %s --add <nafn>\n"

#: ../chkconfig.c:27
#, c-format
msgid "         %s --del <name>\n"
msgstr "         %s --del <nafn>\n"

#: ../chkconfig.c:28
#, c-format
msgid "         %s [--level <levels>] <name> %s)\n"
msgstr "         %s [--level <stig>] <nafn> %s)\n"

#: ../chkconfig.c:35
#, c-format
msgid "service %s does not support chkconfig\n"
msgstr "þjónusta %s styður ekki chkconfig\n"

#: ../chkconfig.c:37
#, c-format
msgid "error reading information on service %s: %s\n"
msgstr "villa við lestur upplýsinga um þjónustu %s: %s\n"

#: ../chkconfig.c:101 ../chkconfig.c:106 ../chkconfig.c:203
msgid "on"
msgstr "á"

#: ../chkconfig.c:101 ../chkconfig.c:106 ../chkconfig.c:203
msgid "off"
msgstr "af"

#: ../chkconfig.c:138
#, c-format
msgid "failed to open %s/init.d: %s\n"
msgstr "gat ekki opnað %s/init.d: %s\n"

#: ../chkconfig.c:172
#, c-format
msgid "error reading from directory %s/init.d: %s"
msgstr "villa við lestur úr möppu %s/init.d: %s"

#: ../chkconfig.c:180
msgid "xinetd based services:\n"
msgstr "þjónustur úr xinetd:\n"

#: ../chkconfig.c:182
#, c-format
msgid "failed to open directory %s: %s"
msgstr "gat ekki opnað möppuna %s: %s"

#: ../chkconfig.c:284
#, c-format
msgid "%s version %s\n"
msgstr "%s útgáfa %s\n"

#: ../chkconfig.c:291
msgid "only one of --list, --add, or --del may be specified\n"
msgstr "einungis má nefna eitt af --list, --add eða --del viðföngunum\n"

#: ../chkconfig.c:339
msgid "only one runlevel may be specified for a chkconfig query\n"
msgstr "einungis má nefna eitt keyrslustig fyrir chkconfig fyrirspurn\n"

#: ../ntsysv.c:30
msgid "Press <F1> for more information on a service."
msgstr "Sláið á <F1> fyrir frekari upplýsingar um þjónustu."

#: ../ntsysv.c:62 ../ntsysv.c:99
msgid "Ok"
msgstr "Í lagi"

#: ../ntsysv.c:62
msgid "Back"
msgstr "Til baka"

#: ../ntsysv.c:62
msgid "Cancel"
msgstr "Hætta við"

#: ../ntsysv.c:74
msgid "What services should be automatically started?"
msgstr "Hvaða þjónustur viltu ræsa sjálfvirkt?"

#: ../ntsysv.c:80
msgid "Services"
msgstr "Þjónustur"

#: ../ntsysv.c:174 ../ntsysv.c:217
#, c-format
msgid "error reading info for service %s: %s\n"
msgstr "villa við lestur upplýsinga um þjónustu %s: %s\n"

#: ../ntsysv.c:183
#, c-format
msgid "error reading from directory %s/init.d: %s\n"
msgstr "villa við lestur úr möppu %s/init.d: %s\n"

#: ../ntsysv.c:226
#, c-format
msgid "error reading from directory %s: %s\n"
msgstr "villa við lestur úr möppunni %s: %s\n"

#: ../ntsysv.c:261
#, c-format
msgid "You must be root to run %s.\n"
msgstr "Þú þarf að vera rót til að keyra %s.\n"

#: ../ntsysv.c:278
msgid "bad argument to --levels\n"
msgstr "óleyfilegt viðfang við --levels\n"

#: ../ntsysv.c:285
msgid "No services may be managed by ntsysv!\n"
msgstr "Engum þjónustum er hægt að stjórna með ntsysv!\n"

#: ../leveldb.c:355
#, c-format
msgid "failed to glob pattern %s: %s\n"
msgstr "gat ekki mátað mynstrið %s: %s\n"

#: ../leveldb.c:386
msgid "cannot determine current run level\n"
msgstr "get ekki ákvarðað núverandi keyrslustig\n"

#: ../leveldb.c:474
#, c-format
msgid "failed to make symlink %s: %s\n"
msgstr "gat ekki búið til tákntengið %s: %s\n"
