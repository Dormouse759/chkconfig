msgid "common options: --verbose --test --help --usage --version\n"
msgstr "sameiginlegir rofar: --verbose --test --help --usage --version\n"

#: ../alternatives.c:58
msgid "                --altdir <directory> --admindir <directory>\n"
msgstr "                     --altdir <mappa> --admindir <mappa>\n"

