#: ../chkconfig.c:21
#, c-format
msgid "%s version %s - Copyright (C) 1997-2000 Red Hat, Inc.\n"
msgstr "%s verze %s - Copyright (C) 1997-2000 Red Hat, Inc.\n"

#: ../chkconfig.c:22
msgid ""
"This may be freely redistributed under the terms of the GNU Public License.\n"
msgstr ""
"Tento program může být volně šířen podle podmínek GNU Public License.\n"

#: ../chkconfig.c:25
#, c-format
msgid "usage:   %s --list [name]\n"
msgstr "použití: %s --list [jméno]\n"

#: ../chkconfig.c:26
#, c-format
msgid "         %s --add <name>\n"
msgstr "         %s --add <jméno>\n"

#: ../chkconfig.c:27
#, c-format
msgid "         %s --del <name>\n"
msgstr "         %s --del <jméno>\n"

#: ../chkconfig.c:28
#, c-format
msgid "         %s [--level <levels>] <name> %s)\n"
msgstr "         %s [--level <úrovně>] <jméno> %s)\n"

#: ../chkconfig.c:35
#, c-format
msgid ""
"service %s supports chkconfig, but is not referenced in any runlevel (run "
"'chkconfig --add %s')\n"
msgstr ""
"služba %s podporuje chkconfig, ale není použita v žádné běhové úrovni (zadejte "
"'chkconfig --add %s')\n"

#: ../chkconfig.c:37
#, c-format
msgid "service %s does not support chkconfig\n"
msgstr "služba %s chkconfig nepodporuje\n"

#: ../chkconfig.c:39
#, c-format
msgid "error reading information on service %s: %s\n"
msgstr "chyba při čtení informací o službě %s: %s\n"

#: ../chkconfig.c:98 ../chkconfig.c:248
#, c-format
msgid "failed to open %s/init.d: %s\n"
msgstr "nelze otevřít %s/init.d: %s\n"

#: ../chkconfig.c:211 ../chkconfig.c:216 ../chkconfig.c:313
msgid "on"
msgstr "on"

#: ../chkconfig.c:211 ../chkconfig.c:216 ../chkconfig.c:313
msgid "off"
msgstr "off"

#: ../chkconfig.c:282
#, c-format
msgid "error reading from directory %s/init.d: %s"
msgstr "chyba při čtení z adresáře %s/init.d: %s"

#: ../chkconfig.c:290
msgid "xinetd based services:\n"
msgstr "služby založené na xinetd:\n"

#: ../chkconfig.c:292
#, c-format
msgid "failed to open directory %s: %s"
msgstr "otevření adresáře selhalo %s: %s"

#: ../chkconfig.c:404
#, c-format
msgid "%s version %s\n"
msgstr "%s verze %s\n"

#: ../chkconfig.c:411
msgid "only one of --list, --add, or --del may be specified\n"
msgstr "může být zadán pouze jeden z přepínačů --list, --add nebo --del\n"

#: ../chkconfig.c:463
msgid "only one runlevel may be specified for a chkconfig query\n"
msgstr "při dotazu může být zadána pouze jedna běhová úroveň\n"

#: ../ntsysv.c:30
msgid "Press <F1> for more information on a service."
msgstr "Stiskněte <F1> pro další informace o službě."

#: ../ntsysv.c:62 ../ntsysv.c:99
msgid "Ok"
msgstr "Ok"

#: ../ntsysv.c:62
msgid "Back"
msgstr "Zpět"

#: ../ntsysv.c:62
msgid "Cancel"
msgstr "Zrušit"

#: ../ntsysv.c:74
msgid "What services should be automatically started?"
msgstr "Které služby mají být automaticky startovány?"

#: ../ntsysv.c:80
msgid "Services"
msgstr "Služby"

#: ../ntsysv.c:186 ../ntsysv.c:230
#, c-format
msgid "error reading info for service %s: %s\n"
msgstr "chyba při čtení informací o službě %s: %s\n"

#: ../ntsysv.c:195
#, c-format
msgid "error reading from directory %s/init.d: %s\n"
msgstr "chyba při čtení z adresáře %s/init.d: %s\n"

#: ../ntsysv.c:239
#, c-format
msgid "error reading from directory %s: %s\n"
msgstr "chyba při čtení z adresáře %s: %s\n"

#: ../ntsysv.c:275
#, c-format
msgid "You must be root to run %s.\n"
msgstr "Musíte být root, abyste mohl spustit %s.\n"

#: ../ntsysv.c:292
msgid "bad argument to --levels\n"
msgstr "chybný argument volby --levels\n"

#: ../ntsysv.c:299
msgid "No services may be managed by ntsysv!\n"
msgstr "Program ntsysv nemůže spravovat žádné služby!\n"

#: ../leveldb.c:507
#, c-format
msgid "failed to glob pattern %s: %s\n"
msgstr "chyba při expandování vzorku %s: %s\n"

#: ../leveldb.c:538
msgid "cannot determine current run level\n"
msgstr "nelze určit aktuální běhovou úroveň\n"

#: ../leveldb.c:626
#, c-format
msgid "failed to make symlink %s: %s\n"
msgstr "nelze vytvořit symbolický odkaz %s: %s\n"
