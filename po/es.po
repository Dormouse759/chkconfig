#: ../chkconfig.c:23
msgid ""
"This may be freely redistributed under the terms of the GNU Public License.\n"
msgstr ""
"Este programa puede ser libremente distribuido bajo los términos de la "
"licencia pública de GNU.\n"

#: ../chkconfig.c:26
#, c-format
msgid "usage:   %s --list [name]\n"
msgstr "uso:   %s --list [nombre]\n"

#: ../chkconfig.c:27
#, c-format
msgid "         %s --add <name>\n"
msgstr "         %s --add <nombre>\n"

#: ../chkconfig.c:28
#, c-format
msgid "         %s --del <name>\n"
msgstr "         %s --del <nombre>\n"

#: ../chkconfig.c:29
#, c-format
msgid "         %s [--level <levels>] <name> %s)\n"
msgstr "         %s [--level <levels>] <nombre> %s)\n"

#: ../chkconfig.c:36
#, c-format
msgid "service %s does not support chkconfig\n"
msgstr "el servicio %s no soporta chkconfig\n"

#: ../chkconfig.c:38
#, c-format
msgid "error reading information on service %s: %s\n"
msgstr "error leyendo la información del servicio %s: %s\n"

#: ../chkconfig.c:100
msgid "on"
msgstr "on"

#: ../chkconfig.c:100
msgid "off"
msgstr "off"

#: ../chkconfig.c:117
#, c-format
msgid "failed to open %s/init.d: %s\n"
msgstr "falló abriendo %s/init.d: %s\n"

#: ../chkconfig.c:151
#, c-format
msgid "error reading from directory %s/init.d: %s"
msgstr "error leyendo del directorio %s/init.d: %s"

#: ../chkconfig.c:231
#, c-format
msgid "%s version %s\n"
msgstr "%s version %s\n"

#: ../chkconfig.c:238
msgid "only one of --list, --add, or --del may be specified\n"
msgstr "solo uno de --list, --add, o --del puede ser especificado\n"

#: ../chkconfig.c:286
msgid "only one runlevel may be specfied for a chkconfig query\n"
msgstr "solo un runlevel puede ser especificado para una consulta de "
"chkconfig\n"

#: ../ntsysv.c:30
msgid "Press <F1> for more information on a service."
msgstr "Pulse <F1> para más información sobre el servicio."

#: ../ntsysv.c:56 ../ntsysv.c:93
msgid "Ok"
msgstr "Ok"

#: ../ntsysv.c:56
msgid "Back"
msgstr "Atrás"

#: ../ntsysv.c:56
msgid "Cancel"
msgstr "Cancelar"

#: ../ntsysv.c:68
msgid "What services should be automatically started?"
msgstr "¿Qué servicios deben ser ejecutados automáticamente?"

#: ../ntsysv.c:74
msgid "Services"
msgstr "Servicios"

#: ../ntsysv.c:164
#, c-format
msgid "error reading info for service %s: %s\n"
msgstr "error leyendo información para el servicio %s: %s.\n"

#: ../ntsysv.c:173
#, c-format
msgid "error reading from directory %s/init.d: %s\n"
msgstr "error leyendo del directorio %s/init.d: %s\n"

#: ../ntsysv.c:219
msgid "bad argument to --levels\n"
msgstr "mal argumento para --levels\n"

#: ../ntsysv.c:226
msgid "No services may be managed by ntsysv!\n"
msgstr "¡Ningún servicio debe ser manejado por ntsysv!\n"

#: ../leveldb.c:265
#, c-format
msgid "failed to glob pattern %s: %s\n"
msgstr "fallo al buscar el patrón %s: %s\n"

