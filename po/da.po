#, c-format
msgid "failed to open directory %s: %s"
msgstr "lykkedes ikke at åbne %s: %s"

#: ../chkconfig.c:284
#, c-format
msgid "%s version %s\n"
msgstr "%s version %s\n"

#: ../chkconfig.c:291
msgid "only one of --list, --add, or --del may be specified\n"
msgstr "kun én af --list, --add eller --del må angives\n"

#: ../chkconfig.c:339
msgid "only one runlevel may be specified for a chkconfig query\n"
msgstr "kun ét runlevel må angives for en chkconfig forespørgsel\n"

#: ../ntsysv.c:30
msgid "Press <F1> for more information on a service."
msgstr "Tast <F1> for mere information om en tjeneste."

#: ../ntsysv.c:62 ../ntsysv.c:99
msgid "Ok"
msgstr "Ok"

#: ../ntsysv.c:62
msgid "Back"
msgstr "Tilbage"

#: ../ntsysv.c:62
msgid "Cancel"
msgstr "Annullér"

#: ../ntsysv.c:74
msgid "What services should be automatically started?"
msgstr "Hvilke tjenester skal startes automatisk?"

#: ../ntsysv.c:80
msgid "Services"
msgstr "Tjenester"

