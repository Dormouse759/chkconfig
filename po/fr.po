# ../chkconfig.c:24
#: ../chkconfig.c:24
msgid "usage:   chkconfig --list [name]\n"
msgstr "Utilisation :   chkconfig --list [nom]\n"

# ../chkconfig.c:25
#: ../chkconfig.c:25
msgid "         chkconfig --add <name>\n"
msgstr "         chkconfig --add <nom>\n"

# ../chkconfig.c:26
#: ../chkconfig.c:26
msgid "         chkconfig --del <name>\n"
msgstr "         chkconfig --del <nom>\n"

# ../chkconfig.c:27
#: ../chkconfig.c:27
msgid "         chkconfig [--level <levels>] <name> <on|off|reset>\n"
msgstr "         chkconfig [--level <niveaux>] <nom> <on|off|reset>\n"

# ../chkconfig.c:49
#: ../chkconfig.c:49
#, c-format
msgid "service %s does not support chkconfig\n"
msgstr "Le service %s ne gère pas chkconfig\n"

# ../chkconfig.c:51
#: ../chkconfig.c:51
#, c-format
msgid "error reading information on service %s: %s\n"
msgstr "Erreur lors de la lecture d'informations sur le service %s : %s\n"

# ../chkconfig.c:93
#: ../chkconfig.c:93
msgid "on"
msgstr "Marche"

# ../chkconfig.c:93
#: ../chkconfig.c:93
msgid "off"
msgstr "Arrêt"

# ../chkconfig.c:109
#: ../chkconfig.c:109
#, c-format
msgid "failed to open %s/init.d: %s\n"
msgstr "Impossible d'ouvrir %s/init.d : %s\n"

# ../chkconfig.c:130 ../ntsysv.c:168
#: ../chkconfig.c:130 ../ntsysv.c:168
#, c-format
msgid "error reading from directory %s/init.d: %s"
