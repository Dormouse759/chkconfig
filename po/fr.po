# ../chkconfig.c:204
#: ../chkconfig.c:204
msgid "only one of --list, --add, or --del may be specified\n"
msgstr "Spécifiez --list, --add OU --del.\n"

# ../chkconfig.c:251
#: ../chkconfig.c:251
msgid "only one runlevel may be specfied for a chkconfig query\n"
msgstr "Vous ne pouvez spécifier qu'un seul niveau d'exécution pour une requête chkconfig\n"

# ../ntsysv.c:30
#: ../ntsysv.c:30
msgid "Press <F1> for more information on a service."
msgstr "Appuyez sur <F1> pour plus d'informations sur un service."

