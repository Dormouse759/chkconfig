#: ../chkconfig.c:21
#, c-format
msgid "%s version %s - Copyright (C) 1997-2000 Red Hat, Inc.\n"
msgstr "%s versio %s - Tekijänoikeudet © 1997-2000 Red Hat, Inc.\n"

#: ../chkconfig.c:22
msgid ""
"This may be freely redistributed under the terms of the GNU Public License.\n"
msgstr ""
"Tätä ohjelmaa saa levittää vapaasti GNU Public Licensen ehtojen mukaisesti.\n"

#: ../chkconfig.c:25
#, c-format
msgid "usage:   %s --list [name]\n"
msgstr "käyttö:  %s --list [nimi]\n"

#: ../chkconfig.c:26
#, c-format
msgid "         %s --add <name>\n"
msgstr "         %s --add <nimi>\n"

#: ../chkconfig.c:27
#, c-format
msgid "         %s --del <name>\n"
msgstr "         %s --del <nimi>\n"

#: ../chkconfig.c:28
#, c-format
msgid "         %s [--level <levels>] <name> %s)\n"
msgstr "         %s [--level <tasot>] <nimi> %s)\n"

#: ../chkconfig.c:35
#, c-format
msgid ""
"service %s supports chkconfig, but is not referenced in any runlevel (run "
"'chkconfig --add %s')\n"
msgstr ""
"palvelulla %s on chkconfig-tuki, mutta siihen ei viitata millään ajotasolla "
"(aja \"chkconfig --add %s\")\n"

#: ../chkconfig.c:37
#, c-format
msgid "service %s does not support chkconfig\n"
msgstr "palvelulla %s ei ole chkconfig-tukea\n"

#: ../chkconfig.c:39
#, c-format
msgid "error reading information on service %s: %s\n"
msgstr "virhe luettaessa palvelun %s tietoja: %s\n"

#: ../chkconfig.c:98 ../chkconfig.c:248
#, c-format
msgid "failed to open %s/init.d: %s\n"
msgstr "tiedoston %s/init.d avaaminen ei onnistunut: %s\n"

#: ../chkconfig.c:211 ../chkconfig.c:216 ../chkconfig.c:313
msgid "on"
msgstr "on"

#: ../chkconfig.c:211 ../chkconfig.c:216 ../chkconfig.c:313
msgid "off"
msgstr "ei"

#: ../chkconfig.c:282
#, c-format
msgid "error reading from directory %s/init.d: %s"
msgstr "virhe luettaessa hakemistosta %s/init.d/: %s"

#: ../chkconfig.c:290
msgid "xinetd based services:\n"
msgstr "xinetd-pohjaiset palvelut:\n"

#: ../chkconfig.c:292
#, c-format
msgid "failed to open directory %s: %s"
msgstr "hakemiston %s avaaminen ei onnistunut: %s"

#: ../chkconfig.c:404
#, c-format
msgid "%s version %s\n"
msgstr "%s versio %s\n"

#: ../chkconfig.c:411
msgid "only one of --list, --add, or --del may be specified\n"
msgstr "vain yksi valitsimista --list, --add tai --del voidaan antaan\n"

#: ../chkconfig.c:463
msgid "only one runlevel may be specified for a chkconfig query\n"
msgstr "chkconfig-kyselyssä voidaan antaa vain yksi ajotaso\n"

#: ../ntsysv.c:30
msgid "Press <F1> for more information on a service."
msgstr "Näppäin <F1> antaa lisää tietoja palvelusta."

#: ../ntsysv.c:62 ../ntsysv.c:99
msgid "Ok"
msgstr "OK"

#: ../ntsysv.c:62
msgid "Back"
msgstr "Takaisin"

#: ../ntsysv.c:62
msgid "Cancel"
msgstr "Peru"

#: ../ntsysv.c:74
msgid "What services should be automatically started?"
msgstr "Mitkä palvelut käynnistetään automaattisesti?"

#: ../ntsysv.c:80
msgid "Services"
msgstr "Palvelut"

#: ../ntsysv.c:186 ../ntsysv.c:230
#, c-format
msgid "error reading info for service %s: %s\n"
msgstr "virhe luettaessa tietoja palvelulle %s: %s\n"

#: ../ntsysv.c:195
#, c-format
msgid "error reading from directory %s/init.d: %s\n"
msgstr "virhe luettaessa hakemistosta %s/init.d: %s\n"

#: ../ntsysv.c:239
#, c-format
msgid "error reading from directory %s: %s\n"
msgstr "virhe luettaessa hakemistosta %s: %s\n"

#: ../ntsysv.c:275
#, c-format
msgid "You must be root to run %s.\n"
msgstr "Vain pääkäyttäjä voi ajaa ohjelman %s.\n"

#: ../ntsysv.c:292
msgid "bad argument to --levels\n"
msgstr "virheellinen argumentti valitsimelle --levels\n"

#: ../ntsysv.c:299
msgid "No services may be managed by ntsysv!\n"
msgstr "Hallittavia palveluita ei löytynyt!\n"

#: ../leveldb.c:507
#, c-format
msgid "failed to glob pattern %s: %s\n"
msgstr "hahmon %s täsmäys epäonnistui: %s\n"

#: ../leveldb.c:538
msgid "cannot determine current run level\n"
msgstr "nykyisen ajotason määritys ei onnistu\n"

#: ../leveldb.c:626
#, c-format
msgid "failed to make symlink %s: %s\n"
msgstr "symlinkin %s tekeminen epäonnistui: %s\n"

#: ../alternatives.c:46
#, c-format
msgid "alternatives version %s - Copyright (C) 2001 Red Hat, Inc.\n"
msgstr "alternatives versio %s - Tekijänoikeudet © 2001 Red Hat, Inc.\n"

#: ../alternatives.c:47
msgid ""
"This may be freely redistributed under the terms of the GNU Public License.\n"
"\n"
msgstr ""
"Tätä ohjelmaa saa levittää vapaasti GNU Public Licensen ehtojen mukaisesti.\n"
"\n"

#: ../alternatives.c:48
msgid "usage: alternatives --install <link> <name> <path> <priority>\n"
msgstr "käyttö: alternatives --install <linkki> <nimi> <polku> <prioriteetti>\n"

#: ../alternatives.c:49
msgid "                    [--initscript <service>]\n"
msgstr "                    [--initscript <palvelu>]\n"

#: ../alternatives.c:50
msgid "                    [--slave <link> <name> <path>]*\n"
msgstr "                    [--slave <linkki> <nimi> <polku>]*\n"

#: ../alternatives.c:51
msgid "       alternatives --remove <name> <path>\n"
msgstr "       alternatives --remove <nimi> <polku>\n"

#: ../alternatives.c:52
msgid "       alternatives --auto <name>\n"
msgstr "       alternatives --auto <nimi>\n"

#: ../alternatives.c:53
msgid "       alternatives --config <name>\n"
msgstr "       alternatives --config <nimi>\n"

#: ../alternatives.c:54
msgid "       alternatives --display <name>\n"
msgstr "       alternatives --display <nimi>\n"

#: ../alternatives.c:55
msgid "       alternatives --set <name> <path>\n"
msgstr "       alternatives --set <nimi> <polku>\n"

#: ../alternatives.c:56
msgid "\n"
msgstr "\n"

#: ../alternatives.c:57
msgid "common options: --verbose --test --help --usage --version\n"
msgstr "yleiset valitsimet: --verbose --test --help --usage --version\n"

#: ../alternatives.c:58
msgid "                --altdir <directory> --admindir <directory>\n"
msgstr "                --altdir <hakemisto> --admindir <hakemisto>\n"

#: ../alternatives.c:159
#, c-format
msgid "failed to open %s: %s\n"
msgstr "tiedoston %s avaaminen epäonnistui: %s\n"

#: ../alternatives.c:168
#, c-format
msgid "failed to read %s: %s\n"
msgstr "tiedoston %s lukeminen epäonnistui: %s\n"

#: ../alternatives.c:177
#, c-format
msgid "%s empty!\n"
msgstr "%s on tyhjä!\n"

#: ../alternatives.c:186
#, c-format
msgid "bad mode on line 1 of %s\n"
msgstr "virheellinen tila rivillä 1 tiedostossa %s\n"

#: ../alternatives.c:193
#, c-format
msgid "bad primary link in %s\n"
msgstr "virheellinen ensisijainen linkki tiedostossa %s\n"

#: ../alternatives.c:205
#, c-format
msgid "path %s unexpected in %s\n"
msgstr "polku %s on odottamaton tiedostossa %s\n"

#: ../alternatives.c:214
#, c-format
msgid "missing path for slave %s in %s\n"
msgstr "puuttuva polku orjalle %s tiedostossa %s\n"

#: ../alternatives.c:224
#, c-format
msgid "unexpected end of file in %s\n"
msgstr "odottamaton tiedoston loppu tiedostossa %s\n"

#: ../alternatives.c:233
#, c-format
msgid "path to alternate expected in %s\n"
msgstr "odotettiin polkua vaihtoehtoon tiedostossa %s\n"

#: ../alternatives.c:253
#, c-format
msgid "numeric priority expected in %s\n"
msgstr "odotettiin numeerista prioriteettia tiedostossa %s\n"

#: ../alternatives.c:275
#, c-format
msgid "slave path expected in %s\n"
msgstr "odotettiin orjapolkua tiedostossa %s\n"

#: ../alternatives.c:294
#, c-format
msgid "unexpected line in %s: %s\n"
msgstr "odottamaton rivi tiedostossa %s: %s\n"

#: ../alternatives.c:316
msgid "link points to no alternative -- setting mode to manual\n"
msgstr "linkki ei osoita vaihtoehtoon -- asetetaan tila käsikäyttöiseksi\n"

#: ../alternatives.c:321
msgid "link changed -- setting mode to manual\n"
msgstr "linkki muutettu -- asetetaan tila käsikäyttöiseksi\n"

#: ../alternatives.c:337 ../alternatives.c:344
#, c-format
msgid "would remove %s\n"
msgstr "poistetettaisiin tiedosto %s\n"

#: ../alternatives.c:339 ../alternatives.c:346 ../alternatives.c:377
#, c-format
msgid "failed to remove link %s: %s\n"
msgstr "linkin %s poistaminen epäonnistui: %s\n"

#: ../alternatives.c:362 ../alternatives.c:374
#, c-format
msgid "would link %s -> %s\n"
msgstr "linkitettäisiin %s -> %s\n"

#: ../alternatives.c:366 ../alternatives.c:383
#, c-format
msgid "failed to link %s -> %s: %s\n"
msgstr "linkin %s -> %s luominen epäonnistui: %s\n"

#: ../alternatives.c:415
#, c-format
msgid "%s already exists\n"
msgstr "%s on jo olemassa\n"

#: ../alternatives.c:417
#, c-format
msgid "failed to create %s: %s\n"
msgstr "tiedoston %s luominen epäonnistui: %s\n"

#: ../alternatives.c:448
#, c-format
msgid "failed to replace %s with %s: %s\n"
msgstr "tiedoston %s korvaaminen tiedostolla %s epäonnistui: %s\n"

#: ../alternatives.c:510
#, c-format
msgid "the primary link for %s must be %s\n"
msgstr "ensisijaisen linkin sovellukselle %s on oltava %s\n"

#: ../alternatives.c:574
#, c-format
msgid "link %s incorrect for slave %s (%s %s)\n"
msgstr "linkki %s on virheellinen orjalle %s (%s %s)\n"

#: ../alternatives.c:616
#, c-format
msgid "%s - status is auto.\n"
msgstr "%s - tila on automaattinen.\n"

#: ../alternatives.c:618
#, c-format
msgid "%s - status is manual.\n"
msgstr "%s - tila on käsikäyttöinen.\n"

#: ../alternatives.c:620
#, c-format
msgid " link currently points to %s\n"
msgstr " linkki viittaa tällä hetkellä tiedostoon %s\n"

#: ../alternatives.c:623
#, c-format
msgid "%s - priority %d\n"
msgstr "%s - prioriteetti %d\n"

#: ../alternatives.c:626
#, c-format
msgid " slave %s: %s\n"
msgstr " orja %s: %s\n"

#: ../alternatives.c:631
#, c-format
msgid "Current `best' version is %s.\n"
msgstr "Nykyinen \"paras\" versio on %s.\n"

#: ../alternatives.c:661
#, c-format
msgid "There are %d programs which provide '%s'.\n"
msgstr "%d ohjelma(a) tarjoaa sovelluksen \"%s\".\n"

#: ../alternatives.c:663
msgid "  Selection    Command\n"
msgstr "  Valinta      Komento\n"

#: ../alternatives.c:672
msgid "Enter to keep the current selection[+], or type selection number: "
msgstr "Enter säilyttää nykyisen valinnan[+], tai anna valintanumero: "

#: ../alternatives.c:675
msgid ""
"\n"
"error reading choice\n"
msgstr ""
"\n"
"virhe luettaessa valintaa\n"

