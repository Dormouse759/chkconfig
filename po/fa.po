#: ../chkconfig.c:50
#, c-format
msgid "usage:   %s [--list] [--type <type>] [name]\n"
msgstr "کاربرد:   %s [--list] [--type <type>] [name]\n"

#: ../chkconfig.c:51
#, c-format
msgid "         %s --add <name>\n"
msgstr "         %s --اضافه کردن <name>\n"

#: ../chkconfig.c:52
#, c-format
msgid "         %s --del <name>\n"
msgstr "         %s --حذف <name>\n"

#: ../chkconfig.c:53
#, c-format
msgid "         %s --override <name>\n"
msgstr "         %s --جایگزین کردن <name>\n"

#: ../chkconfig.c:54
#, c-format
msgid "         %s [--level <levels>] [--type <type>] <name> %s\n"
msgstr "         %s [--level <levels>] [--type <type>] <name> %s\n"

#: ../chkconfig.c:61
#, c-format
msgid ""
"service %s supports chkconfig, but is not referenced in any runlevel (run "
"'chkconfig --add %s')\n"
msgstr ""
"سرویس %s chkconfig  را ساپورت میکند. ولی به تمام Runlevel ها ارجاع نمی شود"
"   (run 'chkconfig --add %s')\n"

