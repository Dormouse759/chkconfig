#: ../chkconfig.c:40
#, c-format
msgid "%s version %s - Copyright (C) 1997-2000 Red Hat, Inc.\n"
msgstr "%s versió %s - Copyright (C) 1997-2000 Red Hat, Inc.\n"

#: ../chkconfig.c:41
#, c-format
msgid ""
"This may be freely redistributed under the terms of the GNU Public "
"License.\n"
msgstr "Aquest programari es pot distribuir lliurement d'acord amb els termes de la Llicència Pública General GNU.\n"

#: ../chkconfig.c:44
#, c-format
msgid "usage:   %s [--list] [--type <type>] [name]\n"
msgstr "forma d'ús:   %s [--list] [--type <tipus>] [nom]\n"

#: ../chkconfig.c:45
#, c-format
msgid "         %s --add <name>\n"
msgstr "         %s --add <nom>\n"

#: ../chkconfig.c:46
#, c-format
msgid "         %s --del <name>\n"
msgstr "         %s –-del <nom>\n"

#: ../chkconfig.c:47
#, c-format
msgid "         %s --override <name>\n"
msgstr "         %s –-override <nom>\n"

#: ../chkconfig.c:48
#, c-format
msgid "         %s [--level <levels>] [--type <type>] <name> %s\n"
msgstr "         %s [--level <nivells>] [--type <tipus>] <nom> %s\n"

#: ../chkconfig.c:56
#, c-format
msgid ""
"\n"
"Note: This output shows SysV services only and does not include native\n"
"      systemd services. SysV configuration data might be overridden by native\n"
"      systemd configuration.\n"
"\n"
msgstr "\nNota: Aquesta sortida només mostra els serveis SysV i no inclueix els serveis\n      natius de systemd. Les dades de configuració de SysV es sobreescriuran per la configuració\n      nativa de systemd.\n"

#: ../chkconfig.c:64
#, c-format
msgid ""
"service %s supports chkconfig, but is not referenced in any runlevel (run "
"'chkconfig --add %s')\n"
msgstr "el servei %s és compatible amb el chkconfig, però no està referenciat en cap nivell d'execució (executeu 'chkconfig –add %s')\n"

#: ../chkconfig.c:66
#, c-format
msgid "service %s does not support chkconfig\n"
msgstr "el servei %s no és compatible amb el chkconfig\n"

#: ../chkconfig.c:68
#, c-format
msgid "error reading information on service %s: %s\n"
msgstr "s'ha produït un error en llegir la informació del servei %s: %s\n"

#: ../chkconfig.c:77
#, c-format
msgid "You do not have enough privileges to perform this operation.\n"
msgstr "No teniu suficients privilegis per executar aquesta operació.\n"

#: ../chkconfig.c:408 ../chkconfig.c:413 ../chkconfig.c:531
msgid "on"
msgstr "executant-se"

#: ../chkconfig.c:408 ../chkconfig.c:413 ../chkconfig.c:531
msgid "off"
msgstr "aturat"

#: ../chkconfig.c:493
#, c-format
msgid "xinetd based services:\n"
msgstr "serveis basats en xinetd:\n"

