#, c-format
msgid "failed to open directory %s: %s\n"
msgstr "no s'ha pogut obrir el directori %s: %s\n"

#: ../chkconfig.c:619
#, c-format
msgid "Note: Forwarding request to 'systemctl %s %s'.\n"
msgstr "Nota: transmetent sol·licitud a «systemctl %s %s».\n"

#: ../chkconfig.c:624
#, c-format
msgid "Failed to forward service request to systemctl: %m\n"
msgstr "S'ha produït un error transmetent la sol·licitud de servei a systemctl: %m\n"

#: ../chkconfig.c:680
#, c-format
msgid "%s version %s\n"
msgstr "%s versió %s\n"

#: ../chkconfig.c:692
#, c-format
msgid "--type must be 'sysv' or 'xinetd'\n"
msgstr "--type ha de ser 'sysv' o 'xinetd'\n"

#: ../chkconfig.c:703
#, c-format
msgid "only one of --list, --add, --del, or --override may be specified\n"
msgstr "només es pot especificar una de les opcions --list, --add, --del, o --override\n"

#: ../chkconfig.c:785
#, c-format
msgid "only one runlevel may be specified for a chkconfig query\n"
msgstr "només es pot especificar un nivell d'execució per a una consulta chkconfig\n"

#: ../ntsysv.c:46
msgid "Press <F1> for more information on a service."
msgstr "Premeu <F1> per a més informació sobre un servei."

#: ../ntsysv.c:78 ../ntsysv.c:115
msgid "Ok"
msgstr "D'acord"

#: ../ntsysv.c:78
msgid "Back"
msgstr "Enrere"

#: ../ntsysv.c:78
msgid "Cancel"
msgstr "Cancel·la"

#: ../ntsysv.c:90
msgid "What services should be automatically started?"
msgstr "Quins serveis s'haurien d'iniciar automàticament?"

#: ../ntsysv.c:96
msgid "Services"
