#: ../chkconfig.c:44
#, c-format
msgid "usage:   %s [--list] [--type <type>] [name]\n"
msgstr "użycie:   %s [--list] [--type <typ>] [nazwa]\n"

#: ../chkconfig.c:45
#, c-format
msgid "         %s --add <name>\n"
msgstr "         %s --add <nazwa>\n"

#: ../chkconfig.c:46
#, c-format
msgid "         %s --del <name>\n"
msgstr "         %s --del <nazwa>\n"

#: ../chkconfig.c:47
#, c-format
msgid "         %s --override <name>\n"
msgstr "         %s --override <nazwa>\n"

#: ../chkconfig.c:48
#, c-format
msgid "         %s [--level <levels>] [--type <type>] <name> %s\n"
msgstr "         %s [--level <poziomy>] [--type <typ>] <nazwa> %s\n"

#: ../chkconfig.c:56
#, c-format
msgid ""
"\n"
"Note: This output shows SysV services only and does not include native\n"
"      systemd services. SysV configuration data might be overridden by native\n"
"      systemd configuration.\n"
"\n"
msgstr "\nUwaga: to wyjście wyświetla tylko usługi SysV i nie zawiera natywnych usług\n       systemd. Dane konfiguracji SysV mogą zostać zastąpione przez natywną\n       konfigurację systemd.\n\n"

#: ../chkconfig.c:64
#, c-format
msgid ""
"service %s supports chkconfig, but is not referenced in any runlevel (run "
"'chkconfig --add %s')\n"
msgstr "usługa %s obsługuje chkconfig, ale nie jest przypisana do żadnego poziomu uruchamiania (proszę wykonać \"chkconfig --add %s\")\n"

#: ../chkconfig.c:66
#, c-format
msgid "service %s does not support chkconfig\n"
msgstr "usługa %s nie obsługuje chkconfig\n"

#: ../chkconfig.c:68
#, c-format
msgid "error reading information on service %s: %s\n"
msgstr "błąd podczas odczytywania informacji o usłudze %s: %s\n"

#: ../chkconfig.c:77
#, c-format
msgid "You do not have enough privileges to perform this operation.\n"
msgstr "Brak wystarczających uprawnień, aby wykonać te działanie.\n"

