msgstr "неможливо відкрити каталог %s: %s\n"

#: ../chkconfig.c:405
#, c-format
msgid "%s version %s\n"
msgstr "%s версія %s\n"

#: ../chkconfig.c:412
msgid "only one of --list, --add, or --del may be specified\n"
msgstr "можна вказувати лише один аргумент з --list, --add або --del\n"

#: ../chkconfig.c:464
msgid "only one runlevel may be specified for a chkconfig query\n"
msgstr "можна вказувати лише один рівень виконання для запиту chkconfig\n"

#: ../ntsysv.c:30
msgid "Press <F1> for more information on a service."
msgstr "Натисніть <F1> для докладнішої інформації про службу."

#: ../ntsysv.c:62 ../ntsysv.c:99
msgid "Ok"
msgstr "Гаразд"

#: ../ntsysv.c:62
msgid "Back"
msgstr "Назад"

