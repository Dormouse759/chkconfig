#: ../chkconfig.c:44
#, c-format
msgid "usage:   %s [--list] [--type <type>] [name]\n"
msgstr "використання:   %s [--list] [--type <тип>] [служба]\n"

#: ../chkconfig.c:45
#, c-format
msgid "         %s --add <name>\n"
msgstr "         %s --add <служба>\n"

#: ../chkconfig.c:46
#, c-format
msgid "         %s --del <name>\n"
msgstr "         %s --del <служба>\n"

#: ../chkconfig.c:47
#, c-format
msgid "         %s --override <name>\n"
msgstr "         %s --override <служба>\n"

#: ../chkconfig.c:48
#, c-format
msgid "         %s [--level <levels>] [--type <type>] <name> %s\n"
msgstr "         %s [--level <рівні>] [--type <тип] <служба> %s\n"

#: ../chkconfig.c:56
#, c-format
msgid ""
"\n"
"Note: This output shows SysV services only and does not include native\n"
"      systemd services. SysV configuration data might be overridden by native\n"
"      systemd configuration.\n"
"\n"
msgstr "\nЗауваження: у виведених даних показано лише служби SysV, там немає типових\n      служб systemd. Дані налаштування SysV могли бути перезаписані типовими\n      налаштуваннями systemd.\n"

#: ../chkconfig.c:64
#, c-format
msgid ""
"service %s supports chkconfig, but is not referenced in any runlevel (run "
"'chkconfig --add %s')\n"
msgstr "служба %s підтримує chkconfig, але не використовується на жодному рівні виконання (запустіть 'chkconfig --add %s')\n"

#: ../chkconfig.c:66
#, c-format
msgid "service %s does not support chkconfig\n"
msgstr "служба %s не підтримує chkconfig\n"

#: ../chkconfig.c:68
#, c-format
msgid "error reading information on service %s: %s\n"
msgstr "помилка читання інформації у службі %s: %s\n"

#: ../chkconfig.c:77
#, c-format
msgid "You do not have enough privileges to perform this operation.\n"
msgstr "У вас недостатньо повноважень для виконання цієї дії.\n"

#: ../chkconfig.c:408 ../chkconfig.c:413 ../chkconfig.c:531
msgid "on"
msgstr "ввімк"

#: ../chkconfig.c:408 ../chkconfig.c:413 ../chkconfig.c:531
msgid "off"
msgstr "вимк"

#: ../chkconfig.c:493
#, c-format
msgid "xinetd based services:\n"
msgstr "служби, що базуються на xinetd:\n"

#: ../chkconfig.c:495
#, c-format
msgid "failed to open directory %s: %s\n"
msgstr "не вдається відкрити каталог %s: %s\n"

#: ../chkconfig.c:619
#, c-format
msgid "Note: Forwarding request to 'systemctl %s %s'.\n"
msgstr "Зауваження: переспрямування запиту до «systemctl %s %s».\n"

#: ../chkconfig.c:624
#, c-format
msgid "Failed to forward service request to systemctl: %m\n"
msgstr "Не вдалося переспрямувати запит щодо обслуговування до systemctl: %m\n"

#: ../chkconfig.c:680
#, c-format
msgid "%s version %s\n"
msgstr "%s версія %s\n"

#: ../chkconfig.c:692
#, c-format
msgid "--type must be 'sysv' or 'xinetd'\n"
msgstr "аргументом --type має бути «sysv» або «xinetd»\n"

#: ../chkconfig.c:703
#, c-format
msgid "only one of --list, --add, --del, or --override may be specified\n"
msgstr "можна вказувати лише один аргумент з --list, --add, --del або --override\n"

#: ../chkconfig.c:785
#, c-format
msgid "only one runlevel may be specified for a chkconfig query\n"
msgstr "можна вказувати лише один рівень виконання для запиту chkconfig\n"

#: ../ntsysv.c:46
msgid "Press <F1> for more information on a service."
msgstr "Натисніть <F1> для докладнішої інформації про службу."

#: ../ntsysv.c:78 ../ntsysv.c:115
