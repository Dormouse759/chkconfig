#: ../chkconfig.c:25
#, c-format
msgid "usage:   %s --list [name]\n"
msgstr "використання:   %s --list [служба]\n"

#: ../chkconfig.c:26
#, c-format
msgid "         %s --add <name>\n"
msgstr "         %s --add <служба>\n"

#: ../chkconfig.c:27
#, c-format
msgid "         %s --del <name>\n"
msgstr "         %s --del <служба>\n"

#: ../chkconfig.c:28
#, c-format
msgid "         %s [--level <levels>] <name> %s)\n"
msgstr "         %s [--level <рівні>] <служба> %s)\n"

#: ../chkconfig.c:35
#, c-format
msgid ""
"service %s supports chkconfig, but is not referenced in any runlevel (run "
"'chkconfig --add %s')\n"
msgstr ""
"служба %s підтримує chkconfig, але не використовується на жодному рівні "
"виконання (запустіть 'chkconfig --add %s')\n"

#: ../chkconfig.c:37
#, c-format
msgid "service %s does not support chkconfig\n"
msgstr "служба %s не підтримує chkconfig\n"

#: ../chkconfig.c:39
#, c-format
msgid "error reading information on service %s: %s\n"
msgstr "помилка читання інформації у службі %s: %s\n"

#: ../chkconfig.c:98 ../chkconfig.c:248
#, c-format
msgid "failed to open %s/init.d: %s\n"
msgstr "неможливо відкрити %s/init.d: %s\n"

#: ../chkconfig.c:211 ../chkconfig.c:216 ../chkconfig.c:313
msgid "on"
msgstr "ввімк"

#: ../chkconfig.c:211 ../chkconfig.c:216 ../chkconfig.c:313
msgid "off"
msgstr "вимк"

#: ../chkconfig.c:282 ../ntsysv.c:195
#, c-format
msgid "error reading from directory %s/init.d: %s\n"
msgstr "помилка читання з каталогу %s/init.d: %s\n"

#: ../chkconfig.c:290
msgid "xinetd based services:\n"
msgstr "служби, що базуються на xinetd:\n"

