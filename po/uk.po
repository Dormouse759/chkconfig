#: ../alternatives.c:48
msgid "usage: alternatives --install <link> <name> <path> <priority>\n"
msgstr "використання: alternatives --install <посилання> <ім'я> <шлях> <пріоритет>\n"

#: ../alternatives.c:49
msgid "                    [--initscript <service>]\n"
msgstr "                    [--initscript <служба>]\n"

#: ../alternatives.c:50
msgid "                    [--slave <link> <name> <path>]*\n"
msgstr "                    [--slave <посилання> <ім'я> <шлях>]*\n"

#: ../alternatives.c:51
msgid "       alternatives --remove <name> <path>\n"
msgstr "       alternatives --remove <ім'я> <шлях>\n"

#: ../alternatives.c:52
msgid "       alternatives --auto <name>\n"
msgstr "       alternatives --auto <ім'я>\n"

#: ../alternatives.c:53
msgid "       alternatives --config <name>\n"
msgstr "       alternatives --config <ім'я>\n"

#: ../alternatives.c:54
msgid "       alternatives --display <name>\n"
msgstr "       alternatives --display <ім'я>\n"

#: ../alternatives.c:55
msgid "       alternatives --set <name> <path>\n"
msgstr "       alternatives --set <ім'я> <шлях>\n"

#: ../alternatives.c:56
msgid "\n"
msgstr "\n"

#: ../alternatives.c:57
msgid "common options: --verbose --test --help --usage --version\n"
msgstr "загальні параметри: --verbose --test --help --usage --version\n"

#: ../alternatives.c:58
msgid "                --altdir <directory> --admindir <directory>\n"
msgstr "                --altdir <directory> --admindir <каталог>\n"

