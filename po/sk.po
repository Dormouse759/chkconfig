#: ../chkconfig.c:21
#, c-format
msgid "%s version %s - Copyright (C) 1997-2000 Red Hat, Inc.\n"
msgstr "%s verzia %s - Autorské práva © 1997-2000 Red Hat, Inc.\n"

#: ../chkconfig.c:22
msgid ""
"This may be freely redistributed under the terms of the GNU Public License.\n"
msgstr ""
"Program môže byť voľne šírený za podmienok určených GNU Public License.\n"

#: ../chkconfig.c:25
#, c-format
msgid "usage:   %s --list [name]\n"
msgstr "použitie:   %s --list [názov]\n"

#: ../chkconfig.c:26
#, c-format
msgid "         %s --add <name>\n"
msgstr "            %s --add <názov>\n"

#: ../chkconfig.c:27
#, c-format
msgid "         %s --del <name>\n"
msgstr "            %s --del <názov>\n"

#: ../chkconfig.c:28
#, c-format
msgid "         %s [--level <levels>] <name> %s)\n"
msgstr "            %s [--level <úrovne>] <názov> %s)\n"

#: ../chkconfig.c:35
#, c-format
msgid ""
"service %s supports chkconfig, but is not referenced in any runlevel (run "
"'chkconfig --add %s')\n"
msgstr ""
"služba %s podporuje chkconfig, ale nie je na ňu odkazované zo žiadnej úrovne "
"(spustite 'chkconfig --add %s')\n"

#: ../chkconfig.c:37
#, c-format
msgid "service %s does not support chkconfig\n"
msgstr "služba %s nepodporuje chkconfig\n"

#: ../chkconfig.c:39
#, c-format
msgid "error reading information on service %s: %s\n"
msgstr "chyba pri čítaní informácií o službe %s: %s\n"

#: ../chkconfig.c:98 ../chkconfig.c:248
#, c-format
msgid "failed to open %s/init.d: %s\n"
msgstr "nepodarilo sa otvoriť %s/init.d: %s\n"

#: ../chkconfig.c:211 ../chkconfig.c:216 ../chkconfig.c:313
msgid "on"
msgstr "zapnuté"

#: ../chkconfig.c:211 ../chkconfig.c:216 ../chkconfig.c:313
msgid "off"
msgstr "vypnuté"

#: ../chkconfig.c:282
#, c-format
msgid "error reading from directory %s/init.d: %s"
msgstr "chyba pri čítaní z adresára %s/init.d: %s"

#: ../chkconfig.c:290
msgid "xinetd based services:\n"
msgstr "služby spúšťané pomocou xinetd:\n"

#: ../chkconfig.c:292
#, c-format
msgid "failed to open directory %s: %s"
msgstr "nepodarilo sa otvoriť adresár %s: %s"

#: ../chkconfig.c:404
#, c-format
msgid "%s version %s\n"
msgstr "%s verzia %s\n"

#: ../chkconfig.c:411
msgid "only one of --list, --add, or --del may be specified\n"
msgstr "môže byť zadaná iba jedna z volieb --list, --add alebo --del\n"

#: ../chkconfig.c:463
msgid "only one runlevel may be specified for a chkconfig query\n"
msgstr "pre chkconfig otázku môže byť zadaná iba jedna úroveň\n"

#: ../ntsysv.c:30
msgid "Press <F1> for more information on a service."
msgstr "Stlačením <F1> získate viac informácií o službe."

#: ../ntsysv.c:62 ../ntsysv.c:99
msgid "Ok"
msgstr "Ok"

#: ../ntsysv.c:62
msgid "Back"
msgstr "Späť"

#: ../ntsysv.c:62
msgid "Cancel"
msgstr "Zrušiť"

#: ../ntsysv.c:74
msgid "What services should be automatically started?"
msgstr "Ktoré služby majú byť automaticky spúšťané?"

#: ../ntsysv.c:80
msgid "Services"
msgstr "Služby"

#: ../ntsysv.c:186 ../ntsysv.c:230
#, c-format
msgid "error reading info for service %s: %s\n"
msgstr "chyba pri čítaní informácií o službe %s: %s\n"

#: ../ntsysv.c:195
#, c-format
msgid "error reading from directory %s/init.d: %s\n"
msgstr "chyba pri čítaní z adresára %s/init.d: %s\n"

#: ../ntsysv.c:239
#, c-format
msgid "error reading from directory %s: %s\n"
msgstr "chyba pri čítaní z adresára %s: %s\n"

#: ../ntsysv.c:275
#, c-format
msgid "You must be root to run %s.\n"
msgstr "Musíte byť správca, aby ste mohli spustiť %s.\n"

#: ../ntsysv.c:292
msgid "bad argument to --levels\n"
msgstr "chybný argument pre --levels\n"

#: ../ntsysv.c:299
msgid "No services may be managed by ntsysv!\n"
msgstr "Žiadne služby nemôžu byť spravované ntsysv!\n"

#: ../leveldb.c:507
#, c-format
msgid "failed to glob pattern %s: %s\n"
msgstr "nepodarilo sa nájsť zhodu so vzorom %s: %s\n"

#: ../leveldb.c:538
msgid "cannot determine current run level\n"
msgstr "nie je možné zistiť aktuálnu úroveň\n"

#: ../leveldb.c:626
#, c-format
msgid "failed to make symlink %s: %s\n"
msgstr "nepodarilo sa vytvoriť symbolický odkaz %s: %s\n"
