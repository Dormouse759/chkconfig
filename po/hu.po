#: ../chkconfig.c:23
msgid ""
"This may be freely redistributed under the terms of the GNU Public License.\n"
msgstr "Ez a szoftver szabadon terjeszthetõ a GNU Public License szerint.\n"

#: ../chkconfig.c:26
#, c-format
msgid "usage:   %s --list [name]\n"
msgstr "usage:   %s --list [név]\n"

#: ../chkconfig.c:27
#, c-format
msgid "         %s --add <name>\n"
msgstr "         %s --add <név>\n"

#: ../chkconfig.c:28
#, c-format
msgid "         %s --del <name>\n"
msgstr "         %s --del <név>\n"

#: ../chkconfig.c:29
#, c-format
msgid "         %s [--level <levels>] <name> %s)\n"
msgstr "         %s [--level <levels>] <név> %s)\n"

#: ../chkconfig.c:36
#, c-format
msgid "service %s does not support chkconfig\n"
msgstr "a %s szolgáltatás nem támogatja a chkconfig használatát\n"

#: ../chkconfig.c:38
#, c-format
msgid "error reading information on service %s: %s\n"
msgstr "információ olvasási hiba a(z) %s szolgáltatásnál: %s\n"

#: ../chkconfig.c:102 ../chkconfig.c:107 ../chkconfig.c:204
msgid "on"
msgstr "be"

#: ../chkconfig.c:102 ../chkconfig.c:107 ../chkconfig.c:204
msgid "off"
msgstr "ki"

#: ../chkconfig.c:139
#, c-format
msgid "failed to open %s/init.d: %s\n"
msgstr "%s/init.d nem megnyitható: %s\n"

#: ../chkconfig.c:173
#, c-format
msgid "error reading from directory %s/init.d: %s"
msgstr "%s/init.d könyvtár nem olvasható: %s"

#: ../chkconfig.c:181
msgid "xinetd based services:\n"
msgstr "xinetd alapú szolgátatások:\n"

#: ../chkconfig.c:183
#, c-format
msgid "failed to open directory %s: %s"
msgstr "%s könyvtár nem megnyitható: %s"

#: ../chkconfig.c:285
#, c-format
msgid "%s version %s\n"
msgstr "%s verzió: %s\n"

#: ../chkconfig.c:292
msgid "only one of --list, --add, or --del may be specified\n"
msgstr "a --list, --add és --del opciók közül csak egyet lehet megadni\n"

#: ../chkconfig.c:340
msgid "only one runlevel may be specified for a chkconfig query\n"
msgstr "csak egy runlevel-t lehet megadni egy chkconfig lekérdezéshez\n"

#: ../ntsysv.c:30
msgid "Press <F1> for more information on a service."
msgstr "Nyomja meg az <F1>-et további információkért a szolgáltatásról."

#: ../ntsysv.c:62 ../ntsysv.c:99
msgid "Ok"
msgstr "Ok"

#: ../ntsysv.c:62
msgid "Back"
msgstr "Vissza"

#: ../ntsysv.c:62
msgid "Cancel"
msgstr "Mégsem"

#: ../ntsysv.c:74
msgid "What services should be automatically started?"
msgstr "Milyen szolgáltatások induljanak automatikusan?"

#: ../ntsysv.c:80
msgid "Services"
msgstr "Szolgáltatások"

#: ../ntsysv.c:174 ../ntsysv.c:217
#, c-format
msgid "error reading info for service %s: %s\n"
msgstr "%s szolgáltatás információ olvasási hiba: %s\n"

#: ../ntsysv.c:183
#, c-format
msgid "error reading from directory %s/init.d: %s\n"
msgstr "%s/init.d könyvtár nem olvasható: %s\n"

#: ../ntsysv.c:226
#, c-format
msgid "error reading from directory %s: %s\n"
msgstr "hiba %s könyvtárból való olvasáskor: %s\n"

#: ../ntsysv.c:270
msgid "bad argument to --levels\n"
msgstr "hibás --levels argumentum\n"

#: ../ntsysv.c:277
msgid "No services may be managed by ntsysv!\n"
msgstr "Az ntsysv semmilyen szolgáltatást nem kezel!\n"

#: ../leveldb.c:355
#, c-format
msgid "failed to glob pattern %s: %s\n"
msgstr "nem lehet kibontani %s: %s\n"

#: ../leveldb.c:386
msgid "cannot determine current run level\n"
msgstr "az aktuális runlevel nem megállapítható\n"

#: ../leveldb.c:474
#, c-format
msgid "failed to make symlink %s: %s\n"
msgstr "%s szimbolikus link nem hozható létre: %s\n"
