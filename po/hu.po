#: ../chkconfig.c:40
#, c-format
msgid "%s version %s - Copyright (C) 1997-2000 Red Hat, Inc.\n"
msgstr "%s %s változat – Szerzői jog (C) Red Hat, Inc., 1997-2000.\n"

#: ../chkconfig.c:41
#, c-format
msgid ""
"This may be freely redistributed under the terms of the GNU Public "
"License.\n"
msgstr "Ez a szoftver szabadon terjeszthető a GNU Public License feltételei szerint.\n"

#: ../chkconfig.c:44
#, c-format
msgid "usage:   %s [--list] [--type <type>] [name]\n"
msgstr "használat:   %s [--list] [--type <típus>] [név]\n"

#: ../chkconfig.c:45
#, c-format
msgid "         %s --add <name>\n"
msgstr "         %s --add <név>\n"

#: ../chkconfig.c:46
#, c-format
msgid "         %s --del <name>\n"
msgstr "         %s --del <név>\n"

#: ../chkconfig.c:47
#, c-format
msgid "         %s --override <name>\n"
msgstr "         %s --override <név>\n"

#: ../chkconfig.c:48
#, c-format
msgid "         %s [--level <levels>] [--type <type>] <name> %s\n"
msgstr "         %s [--level <szintek>] [--type <típus>] <név> %s\n"

#: ../chkconfig.c:56
#, c-format
msgid ""
"\n"
"Note: This output shows SysV services only and does not include native\n"
"      systemd services. SysV configuration data might be overridden by native\n"
"      systemd configuration.\n"
"\n"
msgstr "\nMegjegyzés: Ez a kimenet csak a SysV szolgáltatásokat jeleníti meg és nem tartalmazza a natív\n      systemd szolgáltatásokat. SysV konfigurációs adatok felülírásra kerülhetnek a natív\n      systemd konfigurációkkal.\n\n"

#: ../chkconfig.c:64
#, c-format
msgid ""
"service %s supports chkconfig, but is not referenced in any runlevel (run "
"'chkconfig --add %s')\n"
msgstr "%s szolgáltatás támogatja a chkconfigot, de egyik futási szinten sem szerepel (adja ki a „chkconfig --add %s” parancsot)\n"

#: ../chkconfig.c:66
#, c-format
msgid "service %s does not support chkconfig\n"
msgstr "%s szolgáltatás nem támogatja a chkconfigot\n"

#: ../chkconfig.c:68
#, c-format
msgid "error reading information on service %s: %s\n"
msgstr "hiba történt %s szolgáltatás adatainak beolvasásakor: %s\n"

#: ../chkconfig.c:77
#, c-format
msgid "You do not have enough privileges to perform this operation.\n"
msgstr "Nincs elég joga e művelet elvégzéséhez.\n"

#: ../chkconfig.c:408 ../chkconfig.c:413 ../chkconfig.c:531
msgid "on"
msgstr "be"

#: ../chkconfig.c:408 ../chkconfig.c:413 ../chkconfig.c:531
msgid "off"
msgstr "ki"

#: ../chkconfig.c:493
#, c-format
msgid "xinetd based services:\n"
msgstr "xinetd-alapú szolgáltatások:\n"

#: ../chkconfig.c:495
#, c-format
msgid "failed to open directory %s: %s\n"
msgstr "nem sikerült megnyitni %s könyvtárt: %s\n"

#: ../chkconfig.c:619
#, c-format
msgid "Note: Forwarding request to 'systemctl %s %s'.\n"
msgstr "Megjegyzés: Kérés továbbítva 'systemctl %s %s'.\n"

#: ../chkconfig.c:624
#, c-format
msgid "Failed to forward service request to systemctl: %m\n"
msgstr "Nem sikerült a szolgáltatás kérés továbbítása systemctl-nak: %m\n"

#: ../chkconfig.c:680
#, c-format
msgid "%s version %s\n"
msgstr "%s %s\n"

#: ../chkconfig.c:692
#, c-format
msgid "--type must be 'sysv' or 'xinetd'\n"
msgstr "--type csak „sysv” vagy „xinetd” lehet\n"

#: ../chkconfig.c:703
#, c-format
msgid "only one of --list, --add, --del, or --override may be specified\n"
msgstr "a --list, --add, --del és --override közül csak egyet lehet megadni\n"

#: ../chkconfig.c:785
#, c-format
msgid "only one runlevel may be specified for a chkconfig query\n"
msgstr "csak egy futási szintet lehet megadni egy chkconfig-lekérdezésben\n"

#: ../ntsysv.c:46
msgid "Press <F1> for more information on a service."
msgstr "Egy szolgáltatás jellemzőinek lekérdezéséhez nyomjon <F1>-et!"

#: ../ntsysv.c:78 ../ntsysv.c:115
msgid "Ok"
msgstr "OK"

#: ../ntsysv.c:78
msgid "Back"
msgstr "Vissza"

#: ../ntsysv.c:78
msgid "Cancel"
msgstr "Mégsem"

#: ../ntsysv.c:90
msgid "What services should be automatically started?"
msgstr "Mely szolgáltatások induljanak el automatikusan?"

#: ../ntsysv.c:96
msgid "Services"
msgstr "Szolgáltatások"

#: ../ntsysv.c:181 ../ntsysv.c:208 ../ntsysv.c:245
#, c-format
