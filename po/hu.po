#: ../chkconfig.c:25
#, c-format
msgid "usage:   %s --list [name]\n"
msgstr "Használat:   %s --list [név]\n"

#: ../chkconfig.c:26
#, c-format
msgid "         %s --add <name>\n"
msgstr "         %s --add <név>\n"

#: ../chkconfig.c:27
#, c-format
msgid "         %s --del <name>\n"
msgstr "         %s --del <név>\n"

#: ../chkconfig.c:28
#, c-format
msgid "         %s [--level <levels>] <name> %s)\n"
msgstr "         %s [--level <levels>] <név> %s)\n"

#: ../chkconfig.c:35
#, c-format
msgid ""
"service %s supports chkconfig, but is not referenced in any runlevel (run "
"'chkconfig --add %s')\n"
msgstr ""
"A(z) %s szolgáltatás támogatja a chkconfigot, de egyik futási szinten sem "
"szerepel (adja ki a 'chkconfig --add %s' parancsot)\n"

#: ../chkconfig.c:37
#, c-format
msgid "service %s does not support chkconfig\n"
msgstr "a(z) %s szolgáltatás nem támogatja a chkconfigot\n"

#: ../chkconfig.c:39
#, c-format
msgid "error reading information on service %s: %s\n"
msgstr "hiba történt a(z) %s szolgáltatás adatainak beolvasásakor: %s\n"

#: ../chkconfig.c:98 ../chkconfig.c:248
#, c-format
msgid "failed to open %s/init.d: %s\n"
msgstr "%s/init.d megnyitása nem sikerült: %s\n"

#: ../chkconfig.c:211 ../chkconfig.c:216 ../chkconfig.c:313
msgid "on"
msgstr "be"

#: ../chkconfig.c:211 ../chkconfig.c:216 ../chkconfig.c:313
msgid "off"
msgstr "ki"

#: ../chkconfig.c:282 ../ntsysv.c:195
#, c-format
msgid "error reading from directory %s/init.d: %s\n"
msgstr "hiba történt a(z) %s/init.d könyvtár olvasása közben: %s\n"

#: ../chkconfig.c:290
msgid "xinetd based services:\n"
msgstr "xinetd-alapú szolgátatások:\n"

#: ../chkconfig.c:292
#, c-format
