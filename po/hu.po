#: ../chkconfig.c:21
msgid "This may be freely redistributed under the terms of the GNU Public License.\n"
msgstr "Ez a szoftver szabadon terjeszthet\365 a GNU Public License szerint.\n"

#: ../chkconfig.c:24
msgid "usage:   chkconfig --list [name]\n"
msgstr "usage:   chkconfig --list [n\351v]\n"

#: ../chkconfig.c:25
msgid "         chkconfig --add <name>\n"
msgstr "         chkconfig --add <n\351v>\n"

#: ../chkconfig.c:26
msgid "         chkconfig --del <name>\n"
msgstr "         chkconfig --del <n\351v>\n"

#: ../chkconfig.c:27
msgid "         chkconfig [--level <levels>] <name> <on|off|reset>\n"
msgstr "         chkconfig [--level <levels>] <n\351v> <on|off|reset>\n"

# , c-format
