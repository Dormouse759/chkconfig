msgstr "nem sikerült megnyitni a(z) %s könyvtárt: %s\n"

#: ../chkconfig.c:405
#, c-format
msgid "%s version %s\n"
msgstr "%s %s\n"

#: ../chkconfig.c:412
msgid "only one of --list, --add, or --del may be specified\n"
msgstr "a --list, --add és --del közül csak egyet lehet megadni\n"

#: ../chkconfig.c:464
msgid "only one runlevel may be specified for a chkconfig query\n"
msgstr "csak egy futási szintet lehet megadni egy chkconfig lekérdezésben\n"

#: ../ntsysv.c:30
msgid "Press <F1> for more information on a service."
msgstr "Nyomja meg az <F1>-et egy szolgáltatás jellemzőinek lekérdezéséhez."

#: ../ntsysv.c:62 ../ntsysv.c:99
msgid "Ok"
msgstr "OK"

#: ../ntsysv.c:62
msgid "Back"
msgstr "Vissza"

#: ../ntsysv.c:62
msgid "Cancel"
msgstr "Mégsem"

#: ../ntsysv.c:74
msgid "What services should be automatically started?"
msgstr "Mely szolgáltatások induljanak el automatikusan?"

#: ../ntsysv.c:80
msgid "Services"
msgstr "Szolgáltatások"

#: ../ntsysv.c:186 ../ntsysv.c:230
#, c-format
