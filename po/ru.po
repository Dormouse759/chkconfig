#: ../chkconfig.c:21
#, c-format
msgid "%s version %s - Copyright (C) 1997-2000 Red Hat, Inc.\n"
msgstr "%s version %s - Copyright (C) 1997-2000 Red Hat, Inc.\n"

#: ../chkconfig.c:22
msgid "This may be freely redistributed under the terms of the GNU Public License.\n"
msgstr "Может свободно распостраняться в рамках GNU Public License.\n"

#: ../chkconfig.c:25
#, c-format
msgid "usage:   %s --list [name]\n"
msgstr "применение:   %s --list [имя сервиса]\n"

#: ../chkconfig.c:26
#, c-format
msgid "         %s --add <name>\n"
msgstr "         %s --add <имя сервиса>\n"

#: ../chkconfig.c:27
#, c-format
msgid "         %s --del <name>\n"
msgstr "         %s --del <имя сервиса>\n"

#: ../chkconfig.c:28
#, c-format
msgid "         %s [--level <levels>] <name> %s)\n"
msgstr "         %s [--level <уровни>] <имя сервиса> %s\n"

#: ../chkconfig.c:35
#, c-format
msgid ""
"service %s supports chkconfig, but is not referenced in any runlevel (run "
"'chkconfig --add %s')\n"
msgstr ""
"сервис %s поддерживает chkconfig, но неиспользуется ни в "
"одном уровне выполнения (запустите 'chkconfig --add %s')\n"

#: ../chkconfig.c:37
#, c-format
msgid "service %s does not support chkconfig\n"
msgstr "сервис %s не поддерживает chkconfig\n"

#: ../chkconfig.c:39
#, c-format
msgid "error reading information on service %s: %s\n"
msgstr "ошибка чтения информации о сервисе %s: %s\n"

#: ../chkconfig.c:98 ../chkconfig.c:248
#, c-format
msgid "failed to open %s/init.d: %s\n"
msgstr "невозможно открыть %s/init.d: %s\n"

#: ../chkconfig.c:211 ../chkconfig.c:216 ../chkconfig.c:313
msgid "on"
msgstr "вкл"

#: ../chkconfig.c:211 ../chkconfig.c:216 ../chkconfig.c:313
msgid "off"
msgstr "выкл"

#: ../chkconfig.c:282
#, c-format
msgid "error reading from directory %s/init.d: %s"
msgstr "ошибка чтения из каталога %s/init.d: %s"

#: ../chkconfig.c:290
msgid "xinetd based services:\n"
msgstr "сервисы на основе xinetd:\n"

#: ../chkconfig.c:292
#, c-format
msgid "failed to open directory %s: %s"
msgstr "невозможно открыть каталог %s: %s"

#: ../chkconfig.c:404
#, c-format
msgid "%s version %s\n"
msgstr "%s версия %s\n"

#: ../chkconfig.c:411
msgid "only one of --list, --add, or --del may be specified\n"
msgstr "может быть указана лишь одна команда: --list, --add, или --del\n"

#: ../chkconfig.c:463
msgid "only one runlevel may be specified for a chkconfig query\n"
msgstr "Только один уровень выполнения может быть указан для запроса chkconfig\n"

#: ../ntsysv.c:30
msgid "Press <F1> for more information on a service."
msgstr "Нажмите <F1> для просмотра информации о сервисе."

#: ../ntsysv.c:62 ../ntsysv.c:99
msgid "Ok"
msgstr "OK"

#: ../ntsysv.c:62
msgid "Back"
msgstr "Назад"

#: ../ntsysv.c:62
msgid "Cancel"
msgstr "Отмена"

#: ../ntsysv.c:74
msgid "What services should be automatically started?"
msgstr "Какие сервисы должны запускаться автоматически?"

#: ../ntsysv.c:80
msgid "Services"
msgstr "Сервисы"

#: ../ntsysv.c:186 ../ntsysv.c:230
#, c-format
msgid "error reading info for service %s: %s\n"
msgstr "ошибка чтения информации для сервиса %s: %s\n"

#: ../ntsysv.c:195
#, c-format
msgid "error reading from directory %s/init.d: %s\n"
msgstr "ошибка чтения из директории %s/init.d: %s\n"

#: ../ntsysv.c:239
#, c-format
msgid "error reading from directory %s: %s\n"
msgstr "ошибка чтения из директории %s: %s\n"

#: ../ntsysv.c:275
#, c-format
msgid "You must be root to run %s.\n"
msgstr "Для выполнения %s необходимы права root.\n"

#: ../ntsysv.c:292
msgid "bad argument to --levels\n"
msgstr "неверный аргумент для --levels\n"

#: ../ntsysv.c:299
msgid "No services may be managed by ntsysv!\n"
msgstr "Нет сервисов, управляемых ntsysv!\n"

#: ../leveldb.c:507
#, c-format
msgid "failed to glob pattern %s: %s\n"
msgstr "невозможно использовать шаблон %s: %s\n"

#: ../leveldb.c:538
msgid "cannot determine current run level\n"
msgstr "невозможно определить текущий уровень выполнения\n"

#: ../leveldb.c:626
#, c-format
msgid "failed to make symlink %s: %s\n"
msgstr "ошибка создания символической ссылки %s: %s\n"

