#: ../chkconfig.c:40
#, c-format
msgid "%s version %s - Copyright (C) 1997-2000 Red Hat, Inc.\n"
msgstr "%s версия %s - (C) 1997-2000 Red Hat, Inc.\n"

#: ../chkconfig.c:41
#, c-format
msgid ""
"This may be freely redistributed under the terms of the GNU Public "
"License.\n"
msgstr "Может свободно распространяться в рамках GNU Public License.\n"

#: ../chkconfig.c:44
#, c-format
msgid "usage:   %s [--list] [--type <type>] [name]\n"
msgstr "формат:   %s [--list] [--type <тип>] [имя]\n"

#: ../chkconfig.c:45
#, c-format
msgid "         %s --add <name>\n"
msgstr "         %s --add <имя сервиса>\n"

#: ../chkconfig.c:46
#, c-format
msgid "         %s --del <name>\n"
msgstr "         %s --del <имя сервиса>\n"

#: ../chkconfig.c:47
#, c-format
msgid "         %s --override <name>\n"
msgstr "         %s --override <имя сервиса>\n"

#: ../chkconfig.c:48
#, c-format
msgid "         %s [--level <levels>] [--type <type>] <name> %s\n"
msgstr "         %s [--level <уровни>] [--type <тип>] <имя> %s\n"

#: ../chkconfig.c:55
#, c-format
msgid ""
"service %s supports chkconfig, but is not referenced in any runlevel (run "
"'chkconfig --add %s')\n"
msgstr ""
"сервис %s поддерживает chkconfig, но не используется ни на одном уровне "
"выполнения (запустите 'chkconfig --add %s')\n"

#: ../chkconfig.c:57
#, c-format
msgid "service %s does not support chkconfig\n"
msgstr "сервис %s не поддерживает chkconfig\n"

#: ../chkconfig.c:59
#, c-format
msgid "error reading information on service %s: %s\n"
msgstr "ошибка чтения информации о сервисе %s: %s\n"

#: ../chkconfig.c:68
#, c-format
msgid "You do not have enough privileges to perform this operation.\n"
msgstr "У вас недостаточно полномочий для выполнения этого действия.\n"

#: ../chkconfig.c:391 ../chkconfig.c:396 ../chkconfig.c:517
msgid "on"
msgstr "вкл"

#: ../chkconfig.c:391 ../chkconfig.c:396 ../chkconfig.c:517
msgid "off"
msgstr "выкл"

#: ../chkconfig.c:479
#, c-format
msgid "xinetd based services:\n"
msgstr "службы на основе xinetd:\n"

#: ../chkconfig.c:481
#, c-format
msgid "failed to open directory %s: %s\n"
msgstr "не удалось открыть каталог %s: %s\n"

#: ../chkconfig.c:591
#, c-format
msgid "Note: Forwarding request to 'systemctl %s %s'.\n"
msgstr "Внимание: Отправляется запрос 'systemctl %s %s'.\n"

#: ../chkconfig.c:596
#, c-format
msgid "Failed to forward service request to systemctl: %m\n"
msgstr "Не удалось отправить запрос systemctl: %m\n"

#: ../chkconfig.c:652
#, c-format
msgid "%s version %s\n"
msgstr "%s версия %s\n"

#: ../chkconfig.c:664
#, c-format
msgid "--type must be 'sysv' or 'xinetd'\n"
msgstr "В качестве значения --type надо указать 'sysv' или 'xinetd'\n"

#: ../chkconfig.c:672
#, c-format
msgid "only one of --list, --add, --del, or --override may be specified\n"
msgstr ""
"может быть указана лишь одна команда: --list, --add, --del или --override\n"

#: ../chkconfig.c:722
#, c-format
msgid ""
"\n"
"Note: This output shows SysV services only and does not include native\n"
"      systemd services. SysV configuration data might be overridden by native\n"
"      systemd configuration.\n"
"\n"
msgstr ""
"\n"
"Внимание: в выводе отображены только SysV службы (нативные службы systemd отсутствуют)\n"
"      конфигурация SysV  может быть сброшена нативной\n"
"      конфигураией systemd.\n"
"\n"

#: ../chkconfig.c:759
#, c-format
msgid "only one runlevel may be specified for a chkconfig query\n"
msgstr ""
"Только один уровень выполнения может быть указан для запроса chkconfig\n"

#: ../ntsysv.c:46
msgid "Press <F1> for more information on a service."
msgstr "Нажмите <F1> для просмотра информации о сервисе."

#: ../ntsysv.c:83 ../ntsysv.c:120
msgid "Ok"
msgstr "OK"

#: ../ntsysv.c:83
msgid "Back"
msgstr "Назад"

#: ../ntsysv.c:83
msgid "Cancel"
msgstr "Отмена"

#: ../ntsysv.c:95
msgid "What services should be automatically started?"
msgstr "Какие сервисы должны запускаться автоматически?"

#: ../ntsysv.c:101
msgid "Services"
msgstr "Сервисы"

