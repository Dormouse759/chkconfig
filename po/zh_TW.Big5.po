msgstr "在遵守 GNU Public License 的條款下, 可以自由的散佈這個程式.\n"

#: ../chkconfig.c:25
#, c-format
msgid "usage:   %s --list [name]\n"
msgstr "用法:   %s --list [name]\n"

#: ../chkconfig.c:26
#, c-format
msgid "         %s --add <name>\n"
msgstr "         %s --add <name>\n"

#: ../chkconfig.c:27
#, c-format
msgid "         %s --del <name>\n"
msgstr "         %s --del <name>\n"

#: ../chkconfig.c:28
#, c-format
msgid "         %s [--level <levels>] <name> %s)\n"
msgstr "         %s [--level <levels>] <name> %s)\n"

#: ../chkconfig.c:35
#, c-format
msgid "service %s does not support chkconfig\n"
msgstr "%s 服務不支援 chkconfig\n"

#: ../chkconfig.c:37
#, c-format
msgid "error reading information on service %s: %s\n"
msgstr "在 %s: %s 服務上讀取訊息錯誤\n"

#: ../chkconfig.c:101 ../chkconfig.c:106 ../chkconfig.c:203
msgid "on"
msgstr "開啟"

#: ../chkconfig.c:101 ../chkconfig.c:106 ../chkconfig.c:203
msgid "off"
msgstr "關閉"

#: ../chkconfig.c:138
#, c-format
msgid "failed to open %s/init.d: %s\n"
msgstr "無法開啟 %s/init.d: %s\n"

#: ../chkconfig.c:172
#, c-format
msgid "error reading from directory %s/init.d: %s"
msgstr "讀取 %s/init.d: %s 目錄錯誤"

#: ../chkconfig.c:180
msgid "xinetd based services:\n"
msgstr "以 xinetd 為主的服務:\n"

#: ../chkconfig.c:182
#, c-format
msgid "failed to open directory %s: %s"
msgstr "無法開啟 %s: %s 目錄"

#: ../chkconfig.c:284
#, c-format
msgid "%s version %s\n"
msgstr "%s version %s\n"

#: ../chkconfig.c:291
msgid "only one of --list, --add, or --del may be specified\n"
msgstr "只能指定 --list, --add, 或 --del 中的其中一個\n"

#: ../chkconfig.c:339
msgid "only one runlevel may be specified for a chkconfig query\n"
msgstr "只能對 chkconfig 查詢指定一種執行等級(runlevel)\n"

#: ../ntsysv.c:30
msgid "Press <F1> for more information on a service."
msgstr "請按 <F1> 查看該項服務的更多資訊."

#: ../ntsysv.c:62 ../ntsysv.c:99
msgid "Ok"
msgstr "確定"

#: ../ntsysv.c:62
msgid "Back"
msgstr "上一步"

#: ../ntsysv.c:62
msgid "Cancel"
msgstr "取消"

#: ../ntsysv.c:74
msgid "What services should be automatically started?"
msgstr "那些服務應被自動啟動?"

#: ../ntsysv.c:80
msgid "Services"
msgstr "服務"

#: ../ntsysv.c:174 ../ntsysv.c:218
#, c-format
msgid "error reading info for service %s: %s\n"
msgstr "讀取 %s: %s 服務的資訊錯誤\n"

#: ../ntsysv.c:183
#, c-format
msgid "error reading from directory %s/init.d: %s\n"
msgstr "讀取 %s/init.d: %s 目錄錯誤\n"

#: ../ntsysv.c:227
#, c-format
msgid "error reading from directory %s: %s\n"
msgstr "讀取 %s: %s 目錄錯誤\n"

#: ../ntsysv.c:263
#, c-format
msgid "You must be root to run %s.\n"
msgstr "您必須是系統管理員(root)才能執行 %s \n"

#: ../ntsysv.c:280
msgid "bad argument to --levels\n"
msgstr "在 --levels 處有不當參數\n"

#: ../ntsysv.c:287
msgid "No services may be managed by ntsysv!\n"
msgstr "無任何服務可被 ntsysv 管理!\n"

#: ../leveldb.c:367
#, c-format
msgid "failed to glob pattern %s: %s\n"
msgstr "glob pattern %s: %s 失敗\n"

#: ../leveldb.c:398
msgid "cannot determine current run level\n"
msgstr "無法取得目前的執行等級(runlevel)\n"

#: ../leveldb.c:486
#, c-format
msgid "failed to make symlink %s: %s\n"
msgstr "建立 %s: %s 符號連結(symlink)失敗\n"

