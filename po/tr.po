#: ../chkconfig.c:21
#, c-format
msgid "%s version %s - Copyright (C) 1997-2000 Red Hat, Inc.\n"
msgstr "%s sürüm %s - Telif Hakkı (C) 1997-2002 Red Hat, Inc.\n"

#: ../chkconfig.c:22
msgid "This may be freely redistributed under the terms of the GNU Public License.\n"
msgstr "Bu program GNU kamu lisansı koşulları altında serbestçe dağıtılabilir.\n"

#: ../chkconfig.c:25
#, c-format
msgid "usage:   %s --list [name]\n"
msgstr "kullanımı:   %s --list [isim]\n"

#: ../chkconfig.c:26
#, c-format
msgid "         %s --add <name>\n"
msgstr "             %s --add <isim>\n"

#: ../chkconfig.c:27
#, c-format
msgid "         %s --del <name>\n"
msgstr "             %s --del <isim>\n"

#: ../chkconfig.c:28
#, c-format
msgid "         %s [--level <levels>] <name> %s)\n"
msgstr "             %s [--level <düzeyler>] <isim> %s)\n"

#: ../chkconfig.c:35
#, c-format
msgid ""
"service %s supports chkconfig, but is not registered (run 'chkconfig --add %"
"s')\n"
msgstr ""
"%s chkconfig'i destekliyor, ama kayıtlı değil. ('chkconfig --add %s "
"çalıştırın')\n"

#: ../chkconfig.c:37
#, c-format
msgid "service %s does not support chkconfig\n"
msgstr "%s hizmeti, chkconfig'i desteklemiyor\n"

#: ../chkconfig.c:39
#, c-format
msgid "error reading information on service %s: %s\n"
msgstr "%s hizmeti hakkında bilgi alınırken hata: %s\n"

#: ../chkconfig.c:98 ../chkconfig.c:248
#, c-format
msgid "failed to open %s/init.d: %s\n"
msgstr "%s/init.d açılamıyor: %s\n"

#: ../chkconfig.c:211 ../chkconfig.c:216 ../chkconfig.c:313
msgid "on"
msgstr "açık"

#: ../chkconfig.c:211 ../chkconfig.c:216 ../chkconfig.c:313
msgid "off"
msgstr "kapalı"

#: ../chkconfig.c:282
#, c-format
msgid "error reading from directory %s/init.d: %s"
msgstr "%s/init.d dizininden okunurken hata: %s"

#: ../chkconfig.c:290
msgid "xinetd based services:\n"
msgstr "xinetd tabanlı hizmetler:\n"

#: ../chkconfig.c:292
#, c-format
msgid "failed to open directory %s: %s"
msgstr "%s dizini açılamıyor: %s"

#: ../chkconfig.c:404
#, c-format
msgid "%s version %s\n"
msgstr "%s sürüm %s\n"

#: ../chkconfig.c:411
msgid "only one of --list, --add, or --del may be specified\n"
msgstr "--list, --add veya --del seçeneklerinden sadece biri belirtilebilir\n"

#: ../chkconfig.c:463
msgid "only one runlevel may be specified for a chkconfig query\n"
msgstr "chkconfig sorgulaması için sadece tek bir çalışma düzeyi belirtilebilir\n"

#: ../ntsysv.c:30
msgid "Press <F1> for more information on a service."
msgstr "Hizmet hakkında daha fazla bilgi almak için <F1> tuşuna basın."

#: ../ntsysv.c:62 ../ntsysv.c:99
msgid "Ok"
msgstr "Tamam"

#: ../ntsysv.c:62
msgid "Back"
msgstr "Geri"

#: ../ntsysv.c:62
msgid "Cancel"
msgstr "İptal"

#: ../ntsysv.c:74
msgid "What services should be automatically started?"
msgstr "Hangi hizmetler otomatik olarak çalıştırılsın?"

#: ../ntsysv.c:80
msgid "Services"
msgstr "Hizmetler"

#: ../ntsysv.c:186 ../ntsysv.c:230
#, c-format
msgid "error reading info for service %s: %s\n"
msgstr "%s hizmeti hakkında bilgi alırken hata : %s\n"

#: ../ntsysv.c:195
#, c-format
msgid "error reading from directory %s/init.d: %s\n"
msgstr "%s/init.d dizininden okurken hata: %s\n"

#: ../ntsysv.c:239
#, c-format
msgid "error reading from directory %s: %s\n"
msgstr "%s dizininden okurken hata: %s\n"

#: ../ntsysv.c:275
#, c-format
msgid "You must be root to run %s.\n"
msgstr "%s in çalıştırılabilmesi için root yetkileri gerekiyor.\n"

#: ../ntsysv.c:292
msgid "bad argument to --levels\n"
msgstr "--levels için hatalı argüman\n"

#: ../ntsysv.c:299
msgid "No services may be managed by ntsysv!\n"
msgstr "ntsysv tarafından yönetilebilecek hizmet yok!\n"

#: ../leveldb.c:507
#, c-format
msgid "failed to glob pattern %s: %s\n"
msgstr "kalıp %s genelleştirilemedi: %s\n"

#: ../leveldb.c:538
msgid "cannot determine current run level\n"
msgstr "çalışma düzeyi belirlenemiyor\n"

#: ../leveldb.c:626
#, c-format
msgid "failed to make symlink %s: %s\n"
msgstr "%s sembolik bağı yapılamadı: %s\n"

