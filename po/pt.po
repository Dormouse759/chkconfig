#: ../chkconfig.c:26
#, c-format
msgid "usage:   %s --list [name]\n"
msgstr "uso:   %s --list [nome]\n"

#: ../chkconfig.c:27
#, c-format
msgid "         %s --add <name>\n"
msgstr "         %s --add <nome>\n"

#: ../chkconfig.c:28
#, c-format
msgid "         %s --del <name>\n"
msgstr "         %s --del <nome>\n"

#: ../chkconfig.c:29
#, c-format
msgid "         %s [--level <levels>] <name> %s)\n"
msgstr "         %s [--level <níveis>] <nome> %s)\n"

#: ../chkconfig.c:36
#, c-format
msgid "service %s does not support chkconfig\n"
msgstr "o serviço %s não suporta o chkconfig\n"

#: ../chkconfig.c:38
#, c-format
msgid "error reading information on service %s: %s\n"
msgstr "erro ao ler informação sobre o serviço %s: %s\n"

#: ../chkconfig.c:102 ../chkconfig.c:107 ../chkconfig.c:204
msgid "on"
msgstr "ligado"

#: ../chkconfig.c:102 ../chkconfig.c:107 ../chkconfig.c:204
msgid "off"
msgstr "desligado"

