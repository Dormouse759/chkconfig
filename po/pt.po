#: ../chkconfig.c:23
msgid ""
"This may be freely redistributed under the terms of the GNU Public License.\n"
msgstr ""
"Isto pode ser redistribuído sem custos sob os termos da Licença Pública "
"GNU.\n"

#: ../chkconfig.c:26
#, c-format
msgid "usage:   %s --list [name]\n"
msgstr "uso:   %s --list [nome]\n"

#: ../chkconfig.c:27
#, c-format
msgid "         %s --add <name>\n"
msgstr "         %s --add <nome>\n"

#: ../chkconfig.c:28
#, c-format
msgid "         %s --del <name>\n"
msgstr "         %s --del <nome>\n"

#: ../chkconfig.c:29
#, c-format
msgid "         %s [--level <levels>] <name> %s)\n"
msgstr "         %s [--level <níveis>] <nome> %s)\n"

#: ../chkconfig.c:36
#, c-format
msgid "service %s does not support chkconfig\n"
msgstr "o serviço %s não suporta o chkconfig\n"

#: ../chkconfig.c:38
#, c-format
msgid "error reading information on service %s: %s\n"
msgstr "erro ao ler informação sobre o serviço %s: %s\n"

#: ../chkconfig.c:100
msgid "on"
msgstr "ligado"

#: ../chkconfig.c:100
msgid "off"
msgstr "desligado"

#: ../chkconfig.c:117
#, c-format
msgid "failed to open %s/init.d: %s\n"
msgstr "impossível abrir %s/init.d: %s\n"

#: ../chkconfig.c:151
#, c-format
msgid "error reading from directory %s/init.d: %s"
msgstr "erro ao ler da directoria %s/init.d: %s"

#: ../chkconfig.c:231
#, c-format
msgid "%s version %s\n"
msgstr "%s versão %s\n"

#: ../chkconfig.c:238
msgid "only one of --list, --add, or --del may be specified\n"
msgstr "só um de --list, --add, ou --del pode ser especificado\n"

#: ../chkconfig.c:286
msgid "only one runlevel may be specfied for a chkconfig query\n"
msgstr "só um 'runlevel' pode ser indicado para uma pesquisa do chkconfig\n"

#: ../ntsysv.c:30
msgid "Press <F1> for more information on a service."
msgstr "Carregue <F1> para mais informação sobre um serviço."

#: ../ntsysv.c:56 ../ntsysv.c:93
msgid "Ok"
msgstr "Ok"

#: ../ntsysv.c:56
msgid "Back"
msgstr "Atrás"

#: ../ntsysv.c:56
msgid "Cancel"
msgstr "Cancelar"

#: ../ntsysv.c:68
msgid "What services should be automatically started?"
msgstr "Que serviços devem ser automaticamente iniciados?"

#: ../ntsysv.c:74
msgid "Services"
msgstr "Serviços"

#: ../ntsysv.c:164
#, c-format
msgid "error reading info for service %s: %s\n"
msgstr "erro ao ler informação para o serviço %s: %s\n"

#: ../ntsysv.c:173
#, c-format
msgid "error reading from directory %s/init.d: %s\n"
msgstr "erro ao ler da directoria %s/init.d: %s\n"

#: ../ntsysv.c:219
msgid "bad argument to --levels\n"
msgstr "argumento inválido para --levels\n"

#: ../ntsysv.c:226
msgid "No services may be managed by ntsysv!\n"
msgstr "Nenhuns serviços podem ser geridos pelo ntsysv!\n"

#: ../leveldb.c:265
#, c-format
msgid "failed to glob pattern %s: %s\n"
msgstr "impossível definir o padrão %s: %s\n"

#: ../leveldb.c:296
msgid "cannot determine current run level\n"
msgstr "impossível determinar o 'run level' actual\n"

#: ../leveldb.c:330
#, c-format
msgid "failed to make symlink %s: %s\n"
msgstr "impossível criar o 'link' simbólico %s: %s\n"
