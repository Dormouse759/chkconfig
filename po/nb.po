#: ../chkconfig.c:25
#, c-format
msgid "usage:   %s --list [name]\n"
msgstr "bruk:    %s  --list [navn]\n"

#: ../chkconfig.c:26
#, c-format
msgid "         %s --add <name>\n"
msgstr "         %s --add <navn>\n"

#: ../chkconfig.c:27
#, c-format
msgid "         %s --del <name>\n"
msgstr "         %s --del <navn>\n"

#: ../chkconfig.c:28
#, c-format
msgid "         %s [--level <levels>] <name> %s)\n"
msgstr "         %s [--level <levels>] <navn> %s\n"

#: ../chkconfig.c:35
#, c-format
msgid "service %s does not support chkconfig\n"
msgstr "tjeneste %s støtter ikke chkconfig\n"

#: ../chkconfig.c:37
#, c-format
msgid "error reading information on service %s: %s\n"
msgstr "klarte ikke å finne informasjon om tjeneste %s: %s\n"

#: ../chkconfig.c:101 ../chkconfig.c:106 ../chkconfig.c:203
msgid "on"
msgstr "på"

#: ../chkconfig.c:101 ../chkconfig.c:106 ../chkconfig.c:203
msgid "off"
msgstr "av"

#: ../chkconfig.c:138
#, c-format
msgid "failed to open %s/init.d: %s\n"
msgstr "klarte ikke å åpne %s/init.d: %s\n"

#: ../chkconfig.c:172
#, c-format
msgid "error reading from directory %s/init.d: %s"
msgstr "klarte ikke å lese fra katalog %s/init.d: %s"

#: ../chkconfig.c:180
msgid "xinetd based services:\n"
msgstr "xinetd baserte tjenester:\n"

#: ../chkconfig.c:182
#, c-format
msgid "failed to open directory %s: %s"
msgstr "klarte ikke å åpne katalog %s: %s"

#: ../chkconfig.c:284
#, c-format
msgid "%s version %s\n"
msgstr "%s versjon %s\n"

#: ../chkconfig.c:291
msgid "only one of --list, --add, or --del may be specified\n"
msgstr "du kan bare bruke ett av flaggene --list, --add og --del\n"

#: ../chkconfig.c:339
msgid "only one runlevel may be specified for a chkconfig query\n"
msgstr "du kan bare spesifisere ett kjøringsnivå for en chkconfig spørring\n"

#: ../ntsysv.c:30
msgid "Press <F1> for more information on a service."
msgstr "Trykk <F1> for mer informasjon om en tjeneste."

#: ../ntsysv.c:62 ../ntsysv.c:99
msgid "Ok"
msgstr "Ok"

#: ../ntsysv.c:62
msgid "Back"
msgstr "Tilbake"

#: ../ntsysv.c:62
msgid "Cancel"
msgstr "Avbryt"

#: ../ntsysv.c:74
msgid "What services should be automatically started?"
msgstr "Hvilke tjenester skal startes automatisk?"

#: ../ntsysv.c:80
msgid "Services"
msgstr "Tjenester"

#: ../ntsysv.c:174 ../ntsysv.c:217
#, c-format
msgid "error reading info for service %s: %s\n"
msgstr "feil under lesing av informasjon for tjeneste %s: %s\n"

#: ../ntsysv.c:183
#, c-format
msgid "error reading from directory %s/init.d: %s\n"
msgstr "feil under lesing fra katalog %s/init.d: %s\n"

#: ../ntsysv.c:226
#, c-format
msgid "error reading from directory %s: %s\n"
msgstr "feil under lesing fra katalog %s: %s\n"

#: ../ntsysv.c:270
msgid "bad argument to --levels\n"
msgstr "galt argument til --levels\n"

#: ../ntsysv.c:277
msgid "No services may be managed by ntsysv!\n"
msgstr "Ingen av tjenestene kan styres av ntsysv!\n"

