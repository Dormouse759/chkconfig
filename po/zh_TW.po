msgid "This may be freely redistributed under the terms of the GNU Public License.\n"
msgstr "在遵守 GNU Public License 的條款下，可以自由的散佈這個程式。\n"

#: ../chkconfig.c:25
#, c-format
msgid "usage:   %s --list [name]\n"
msgstr "用法:   %s --list [name]\n"

#: ../chkconfig.c:26
#, c-format
msgid "         %s --add <name>\n"
msgstr "         %s --add <name>\n"

#: ../chkconfig.c:27
#, c-format
msgid "         %s --del <name>\n"
msgstr "         %s --del <name>\n"

#: ../chkconfig.c:28
#, c-format
msgid "         %s [--level <levels>] <name> %s)\n"
msgstr "         %s [--level <levels>] <name> %s)\n"

#: ../chkconfig.c:35
#, c-format
msgid ""
"service %s supports chkconfig, but is not referenced in any runlevel (run "
"'chkconfig --add %s')\n"
msgstr "%s 服務支援 chkconfig，但未向任何執行等級註冊 (請執行 'chkconfig --add %s')\n"

#: ../chkconfig.c:37
#, c-format
msgid "service %s does not support chkconfig\n"
msgstr "%s 服務不支援 chkconfig\n"

#: ../chkconfig.c:39
#, c-format
msgid "error reading information on service %s: %s\n"
msgstr "在 %s 服務上讀取訊息錯誤: %s\n"

#: ../chkconfig.c:98 ../chkconfig.c:248
#, c-format
msgid "failed to open %s/init.d: %s\n"
msgstr "無法開啟 %s/init.d: %s\n"

#: ../chkconfig.c:211 ../chkconfig.c:216 ../chkconfig.c:313
msgid "on"
msgstr "開啟"

#: ../chkconfig.c:211 ../chkconfig.c:216 ../chkconfig.c:313
msgid "off"
msgstr "關閉"

#: ../chkconfig.c:282
#, c-format
msgid "error reading from directory %s/init.d: %s"
msgstr "讀取 %s/init.d 目錄錯誤: %s"

#: ../chkconfig.c:290
msgid "xinetd based services:\n"
msgstr "以 xinetd 為主的服務:\n"

