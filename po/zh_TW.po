#, c-format
msgid "failed to open directory %s: %s"
msgstr "無法開啟 %s 目錄: %s"

#: ../chkconfig.c:404
#, c-format
msgid "%s version %s\n"
msgstr "%s version %s\n"

#: ../chkconfig.c:411
msgid "only one of --list, --add, or --del may be specified\n"
msgstr "只能指定 --list, --add, 或 --del 中的其中一個\n"

#: ../chkconfig.c:463
msgid "only one runlevel may be specified for a chkconfig query\n"
msgstr "只能對 chkconfig 查詢指定一種執行等級\n"

#: ../ntsysv.c:30
msgid "Press <F1> for more information on a service."
msgstr "請按 <F1> 查看該項服務的更多資訊。"

#: ../ntsysv.c:62 ../ntsysv.c:99
msgid "Ok"
msgstr "確定"

#: ../ntsysv.c:62
msgid "Back"
msgstr "上一步"

#: ../ntsysv.c:62
msgid "Cancel"
msgstr "取消"

#: ../ntsysv.c:74
msgid "What services should be automatically started?"
msgstr "那些服務應被自動啟動？"

#: ../ntsysv.c:80
msgid "Services"
msgstr "服務"

#: ../ntsysv.c:186 ../ntsysv.c:230
#, c-format
msgid "error reading info for service %s: %s\n"
msgstr "讀取 %s 服務的資訊錯誤: %s\n"

#: ../ntsysv.c:195
#, c-format
msgid "error reading from directory %s/init.d: %s\n"
msgstr "讀取 %s/init.d 目錄錯誤: %s\n"

#: ../ntsysv.c:239
#, c-format
msgid "error reading from directory %s: %s\n"
msgstr "讀取 %s 目錄錯誤: %s\n"

#: ../ntsysv.c:275
#, c-format
msgid "You must be root to run %s.\n"
msgstr "您必須是 root 才能執行 %s。\n"

#: ../ntsysv.c:292
msgid "bad argument to --levels\n"
msgstr "在 --levels 處有不當參數\n"

#: ../ntsysv.c:299
msgid "No services may be managed by ntsysv!\n"
msgstr "無任何服務可被 ntsysv 管理！\n"

#: ../leveldb.c:507
#, c-format
msgid "failed to glob pattern %s: %s\n"
msgstr "glob pattern %s 失敗: %s\n"

#: ../leveldb.c:538
msgid "cannot determine current run level\n"
msgstr "無法取得目前的執行等級\n"

#: ../leveldb.c:626
#, c-format
msgid "failed to make symlink %s: %s\n"
msgstr "建立 %s 符號連結失敗: %s\n"

#: ../alternatives.c:46
#, c-format
msgid "alternatives version %s - Copyright (C) 2001 Red Hat, Inc.\n"
msgstr "另一版本 %s - 版權 (C) 2001 Red Hat, Inc.\n"

#: ../alternatives.c:47
msgid ""
"This may be freely redistributed under the terms of the GNU Public License.\n"
"\n"
msgstr "在遵守 GNU 通用公共許可證的條款下，可以自由的散佈這個程式。\n"

#: ../alternatives.c:48
msgid "usage: alternatives --install <link> <name> <path> <priority>\n"
msgstr "用法: alternatives --install <連結> <名稱> <路徑> <優先順序>\n"

#: ../alternatives.c:49
msgid "                    [--initscript <service>]\n"
msgstr "                    [--initscript <服務名稱>]\n"

#: ../alternatives.c:50
msgid "                    [--slave <link> <name> <path>]*\n"
msgstr "                    [--slave <連結> <名稱> <路徑>]*\n"

#: ../alternatives.c:51
msgid "       alternatives --remove <name> <path>\n"
msgstr "       alternatives --remove <名稱> <路徑>\n"

#: ../alternatives.c:52
msgid "       alternatives --auto <name>\n"
msgstr "       alternatives --auto <名稱>\n"

#: ../alternatives.c:53
msgid "       alternatives --config <name>\n"
msgstr "       alternatives --config <名稱>\n"

#: ../alternatives.c:54
msgid "       alternatives --display <name>\n"
msgstr "       alternatives --display <名稱>\n"

#: ../alternatives.c:55
msgid "       alternatives --set <name> <path>\n"
msgstr "       alternatives --set <名稱> <路徑>\n"

#: ../alternatives.c:56
msgid "\n"
msgstr "\n"

#: ../alternatives.c:57
msgid "common options: --verbose --test --help --usage --version\n"
msgstr "共同選項: --verbose --test --help --usage --version\n"

#: ../alternatives.c:58
msgid "                --altdir <directory> --admindir <directory>\n"
msgstr "                --altdir <目錄> --admindir <目錄>\n"

#: ../alternatives.c:159
#, c-format
msgid "failed to open %s: %s\n"
msgstr "無法開啟 %s: %s\n"

#: ../alternatives.c:168
#, c-format
msgid "failed to read %s: %s\n"
msgstr "無法讀取 %s: %s\n"

#: ../alternatives.c:177
#, c-format
msgid "%s empty!\n"
msgstr "%s 是空的!\n"

#: ../alternatives.c:186
#, c-format
msgid "bad mode on line 1 of %s\n"
msgstr "%s 的第一行有不當的模式\n"

#: ../alternatives.c:193
#, c-format
msgid "bad primary link in %s\n"
msgstr "%s 有不當的主要連結\n"

#: ../alternatives.c:205
#, c-format
msgid "path %s unexpected in %s\n"
msgstr "%s 路徑是錯誤的在 %s 中\n"

#: ../alternatives.c:214
#, c-format
msgid "missing path for slave %s in %s\n"
msgstr "遺失從屬裝置 %s 的路徑在 %s 中\n"

#: ../alternatives.c:224
#, c-format
msgid "unexpected end of file in %s\n"
msgstr "在 %s 中的檔案結尾有錯誤\n"

#: ../alternatives.c:233
#, c-format
msgid "path to alternate expected in %s\n"
msgstr "在 %s 中請輸入至 alternate 的路徑\n"

#: ../alternatives.c:253
#, c-format
msgid "numeric priority expected in %s\n"
msgstr "請輸入數值的優先順序到 %s\n"

#: ../alternatives.c:275
#, c-format
msgid "slave path expected in %s\n"
msgstr "請輸入從屬裝置路徑到 %s\n"

#: ../alternatives.c:294
#, c-format
msgid "unexpected line in %s: %s\n"
msgstr "%s 中有錯誤的行列: %s\n"

#: ../alternatives.c:316
msgid "link points to no alternative -- setting mode to manual\n"
msgstr "連結未指向任何 alternative -- 設定為手動模式\n"

#: ../alternatives.c:321
msgid "link changed -- setting mode to manual\n"
msgstr "連結已改變 -- 設定為手動模式\n"

#: ../alternatives.c:337 ../alternatives.c:344
#, c-format
msgid "would remove %s\n"
msgstr "將移除 %s\n"

#: ../alternatives.c:339 ../alternatives.c:346 ../alternatives.c:377
#, c-format
msgid "failed to remove link %s: %s\n"
msgstr "移除 %s 的連結失敗: %s\n"

#: ../alternatives.c:362 ../alternatives.c:374
#, c-format
msgid "would link %s -> %s\n"
msgstr "將連結 %s -> %s\n"

#: ../alternatives.c:366 ../alternatives.c:383
#, c-format
msgid "failed to link %s -> %s: %s\n"
msgstr "建立 %s -> %s 的連結失敗: %s\n"

#: ../alternatives.c:415
#, c-format
msgid "%s already exists\n"
msgstr "%s 已經存在\n"

#: ../alternatives.c:417
#, c-format
msgid "failed to create %s: %s\n"
msgstr "建立 %s 失敗: %s\n"

#: ../alternatives.c:448
#, c-format
msgid "failed to replace %s with %s: %s\n"
msgstr "取代 %s 以 %s 失敗: %s\n"

#: ../alternatives.c:510
#, c-format
msgid "the primary link for %s must be %s\n"
msgstr "%s 的主要連結必須是 %s\n"

#: ../alternatives.c:574
#, c-format
msgid "link %s incorrect for slave %s (%s %s)\n"
msgstr "%s 連結在從屬裝置 %s 上是錯誤的 (%s %s)\n"

#: ../alternatives.c:616
#, c-format
msgid "%s - status is auto.\n"
msgstr "%s - 狀態是自動的。\n"

#: ../alternatives.c:618
#, c-format
msgid "%s - status is manual.\n"
msgstr "%s - 狀態是手動的。\n"

#: ../alternatives.c:620
#, c-format
msgid " link currently points to %s\n"
msgstr " 目前連結指向 %s\n"

#: ../alternatives.c:623
#, c-format
msgid "%s - priority %d\n"
msgstr "%s - 優先順序 %d\n"

#: ../alternatives.c:626
#, c-format
msgid " slave %s: %s\n"
msgstr " 從屬裝置 %s: %s\n"

#: ../alternatives.c:631
#, c-format
msgid "Current `best' version is %s.\n"
msgstr "目前 `最好的' 版本是 %s。\n"

#: ../alternatives.c:661
#, c-format
msgid "There are %d programs which provide '%s'.\n"
msgstr "有 %d 程式提供 '%s'。\n"

#: ../alternatives.c:663
msgid "  Selection    Command\n"
msgstr "  選擇        指令\n"

#: ../alternatives.c:672
msgid "Enter to keep the current selection[+], or type selection number: "
msgstr "請輸入以保留目前的選擇[+]，或輸入選擇號碼:"

#: ../alternatives.c:675
msgid ""
"\n"
"error reading choice\n"
msgstr ""
"\n"
"讀取選擇發生錯誤\n"

