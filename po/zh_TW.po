#: ../chkconfig.c:21
#, c-format
msgid "%s version %s - Copyright (C) 1997-2000 Red Hat, Inc.\n"
msgstr "%s 版本 %s - 版權 (C) 1997-2000 Red Hat, Inc.\n"

