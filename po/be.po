#: ../chkconfig.c:21
#, c-format
msgid "%s version %s - Copyright (C) 1997-2000 Red Hat, Inc.\n"
msgstr "%s вэрсіі %s - Copyright (C) 1997-2000 Red Hat, Inc.\n"

#: ../chkconfig.c:22
msgid "This may be freely redistributed under the terms of the GNU Public License.\n"
msgstr "Можа вольна распаўсюджвацца ў межах GNU Public License.\n"

#: ../chkconfig.c:25
#, c-format
msgid "usage:   %s --list [name]\n"
msgstr "выкарыстаньне:   %s --list [імя сэрвісу]\n"

#: ../chkconfig.c:26
#, c-format
msgid "         %s --add <name>\n"
msgstr "         %s --add <імя сэрвісу>\n"

#: ../chkconfig.c:27
#, c-format
msgid "         %s --del <name>\n"
msgstr "         %s --del <імя сэрвісу>\n"

#: ../chkconfig.c:28
#, c-format
msgid "         %s [--level <levels>] <name> %s\n"
msgstr "         %s [--level <узроўні>] <імя сэрвісу> %s\n"

#: ../chkconfig.c:35
#, c-format
msgid ""
"service %s supports chkconfig, but is not referenced in any runlevel (run "
"'chkconfig --add %s')\n"
msgstr ""
"сэрвіс %s падтрымлівае chkconfig, але не выкарыстоўваецца ні на адным з узроўняў "
"выкананьня (запусьціце 'chkconfig --add %s')\n"

#: ../chkconfig.c:37
#, c-format
msgid "service %s does not support chkconfig\n"
msgstr "сэрвіс %s не падтрымлівае chkconfig\n"

#: ../chkconfig.c:39
#, c-format
msgid "error reading information on service %s: %s\n"
msgstr "памылка чытаньня інфармацыі аб сэрвісе %s: %s\n"

#: ../chkconfig.c:98 ../chkconfig.c:248
#, c-format
msgid "failed to open %s/init.d: %s\n"
msgstr "збой адкрыцьця %s/init.d: %s\n"

#: ../chkconfig.c:211 ../chkconfig.c:216 ../chkconfig.c:313
msgid "on"
msgstr "укл"

#: ../chkconfig.c:211 ../chkconfig.c:216 ../chkconfig.c:313
msgid "off"
msgstr "адкл"

#: ../chkconfig.c:282 ../ntsysv.c:195
#, c-format
msgid "error reading from directory %s/init.d: %s\n"
msgstr "памылка чытаньня з дырэкторыі %s/init.d: %s\n"

#: ../chkconfig.c:290
msgid "xinetd based services:\n"
msgstr "сэрвісы на базе xinetd:\n"

#: ../chkconfig.c:292
#, c-format
msgid "failed to open directory %s: %s\n"
msgstr "не атрымалася адкрыць каталёг %s: %s\n"

#: ../chkconfig.c:405
#, c-format
msgid "%s version %s\n"
msgstr "%s вэрсія %s\n"

#: ../chkconfig.c:412
msgid "only one of --list, --add, or --del may be specified\n"
msgstr "можа быць пазначана толькі адна каманда: --list, --add, ці --del\n"

#: ../chkconfig.c:464
msgid "only one runlevel may be specified for a chkconfig query\n"
msgstr "Толькі адзін узровень выкананьня можа быць пазначаны для запыту chkconfig\n"

#: ../ntsysv.c:30
msgid "Press <F1> for more information on a service."
msgstr "Націсьніце <F1> для прагляду зьвестак аб сэрвісе."

#: ../ntsysv.c:62 ../ntsysv.c:99
msgid "Ok"
msgstr "Добра"

#: ../ntsysv.c:62
msgid "Back"
msgstr "Назад"

#: ../ntsysv.c:62
msgid "Cancel"
msgstr "Скасаваць"

#: ../ntsysv.c:74
msgid "What services should be automatically started?"
msgstr "Якія сэрвісы мусяць запускацца аўтаматычна?"

#: ../ntsysv.c:80
msgid "Services"
msgstr "Сэрвісы"

#: ../ntsysv.c:186 ../ntsysv.c:230
#, c-format
msgid "error reading info for service %s: %s\n"
msgstr "памылка чытаньня інфармацыі для сэрвісу %s: %s\n"

#: ../ntsysv.c:239
#, c-format
msgid "error reading from directory %s: %s\n"
msgstr "памылка чытаньня з дырэкторыі %s: %s\n"

#: ../ntsysv.c:275
#, c-format
msgid "You must be root to run %s.\n"
msgstr "Для выкананьня %s неабходныя правы root.\n"

#: ../ntsysv.c:292
msgid "bad argument to --levels\n"
msgstr "памылковы аргумэнт для --levels\n"

#: ../ntsysv.c:299
msgid "No services may be managed by ntsysv!\n"
msgstr "Няма сэрвісаў, якія кіруюцца ntsysv!\n"

#: ../leveldb.c:508
#, c-format
msgid "failed to glob pattern %s: %s\n"
msgstr "немагчыма выкарыстоўваць шаблён %s: %s\n"

#: ../leveldb.c:539
msgid "cannot determine current run level\n"
msgstr "немагчыма вызначыць бягучы узровень выкананьня\n"

#: ../leveldb.c:627
#, c-format
msgid "failed to make symlink %s: %s\n"
msgstr "памылка стварэньня сымбалічнай спасылкі %s: %s\n"

#: ../alternatives.c:48
#, c-format
msgid "alternatives version %s - Copyright (C) 2001 Red Hat, Inc.\n"
msgstr "alternatives вэрсія %s - Copyright (C) 2001 Red Hat, Inc.\n"

#: ../alternatives.c:49
msgid ""
"This may be freely redistributed under the terms of the GNU Public License.\n"
"\n"
msgstr ""
"Можа вольна распаўсюджвацца ў межах GNU Public License.\n"
"\n"

#: ../alternatives.c:50
msgid "usage: alternatives --install <link> <name> <path> <priority>\n"
msgstr "выкарыстаньне: alternatives --install <спасылка> <назва> <шлях> <прыярытэт>\n"

#: ../alternatives.c:51
msgid "                    [--initscript <service>]\n"
msgstr "                    [--initscript <служба>]\n"

#: ../alternatives.c:52
msgid "                    [--slave <link> <name> <path>]*\n"
msgstr "                    [--slave <спасылка> <назва> <шлях>]*\n"

#: ../alternatives.c:53
msgid "       alternatives --remove <name> <path>\n"
msgstr "       alternatives --remove <назва> <шлях>\n"

#: ../alternatives.c:54
msgid "       alternatives --auto <name>\n"
msgstr "       alternatives --auto <назва>\n"

#: ../alternatives.c:55
msgid "       alternatives --config <name>\n"
msgstr "       alternatives --config <назва>\n"

#: ../alternatives.c:56
msgid "       alternatives --display <name>\n"
msgstr "       alternatives --display <назва>\n"

#: ../alternatives.c:57
msgid "       alternatives --set <name> <path>\n"
msgstr "       alternatives --set <назва> <шлях>\n"

#: ../alternatives.c:58
msgid "\n"
msgstr "\n"

#: ../alternatives.c:59
msgid "common options: --verbose --test --help --usage --version\n"
msgstr "агульныя парамэтры: --verbose --test --help --usage --version\n"

#: ../alternatives.c:60
msgid "                --altdir <directory> --admindir <directory>\n"
msgstr "                --altdir <каталёг> --admindir <каталёг>\n"

