#: ../chkconfig.c:25
#, c-format
msgid "usage:   %s --list [name]\n"
msgstr "gebruik: %s --list [naam]\n"

#: ../chkconfig.c:26
#, c-format
msgid "         %s --add <name>\n"
msgstr "         %s --add <naam>\n"

#: ../chkconfig.c:27
#, c-format
msgid "         %s --del <name>\n"
msgstr "         %s --del <naam>\n"

#: ../chkconfig.c:28
#, c-format
msgid "         %s [--level <levels>] <name> %s)\n"
msgstr "         %s [--level <levels>] <naam> %s)\n"

#: ../chkconfig.c:35
#, c-format
msgid ""
"service %s supports chkconfig, but is not referenced in any runlevel (run "
"'chkconfig --add %s')\n"
msgstr ""
"service %s ondersteunt chkconfig, maar in geen enkel runlevel staat een "
"verwijzing naar deze service (uitvoeren 'chkconfig --add %s')\n"

#: ../chkconfig.c:37
#, c-format
msgid "service %s does not support chkconfig\n"
msgstr "service %s ondersteunt chkconfig niet\n"

#: ../chkconfig.c:39
#, c-format
msgid "error reading information on service %s: %s\n"
msgstr "fout bij lezen informatie over service %s: %s\n"

