#: ../chkconfig.c:21
#, c-format
msgid "%s version %s - Copyright (C) 1997-2000 Red Hat, Inc.\n"
msgstr "%s versione %s - Copyright (C) 1997-2000 Red Hat, Inc.\n"

#: ../chkconfig.c:22
msgid ""
"This may be freely redistributed under the terms of the GNU Public License.\n"
msgstr ""
"Questo software può essere distribuito liberamente secondo la Licenza "
"pubblica GNU.\n"

#: ../chkconfig.c:25
#, c-format
msgid "usage:   %s --list [name]\n"
msgstr "usare:  %s --list [nome]\n"

#: ../chkconfig.c:26
#, c-format
msgid "         %s --add <name>\n"
msgstr "         %s --aggiungere <nome>\n"

#: ../chkconfig.c:27
#, c-format
msgid "         %s --del <name>\n"
msgstr "         %s --del <nome>\n"

#: ../chkconfig.c:28
#, c-format
msgid "         %s [--level <levels>] <name> %s)\n"
msgstr "         %s  [--level] <livelli>] <nome> %s)\n"

#: ../chkconfig.c:35
#, c-format
msgid ""
"service %s supports chkconfig, but is not referenced in any runlevel (run "
"'chkconfig --add %s')\n"
msgstr ""
"il servizio %s supporta chkconfig, ma non vi è traccia nel runlevel "
"(eseguire 'chkconfig --add %s')\n"

#: ../chkconfig.c:37
#, c-format
msgid "service %s does not support chkconfig\n"
msgstr "Il servizio %s non supporta chkconfig\n"

#: ../chkconfig.c:39
#, c-format
msgid "error reading information on service %s: %s\n"
msgstr "errore durante la lettura delle informazioni sul servizio %s: %s\n"

#: ../chkconfig.c:98 ../chkconfig.c:248
#, c-format
msgid "failed to open %s/init.d: %s\n"
msgstr "impossibile aprire %s/init.d: %s\n"

#: ../chkconfig.c:211 ../chkconfig.c:216 ../chkconfig.c:313
msgid "on"
msgstr "on"

#: ../chkconfig.c:211 ../chkconfig.c:216 ../chkconfig.c:313
msgid "off"
msgstr "off"

#: ../chkconfig.c:282
#, c-format
msgid "error reading from directory %s/init.d: %s"
msgstr "errore durante la lettura dalla directory %s/init.d: %s"

#: ../chkconfig.c:290
msgid "xinetd based services:\n"
msgstr "servizi basati su xinetd:\n"

#: ../chkconfig.c:292
#, c-format
msgid "failed to open directory %s: %s"
msgstr "impossibile aprire la directory %s: %s"

#: ../chkconfig.c:404
#, c-format
msgid "%s version %s\n"
msgstr "%s versione %s\n"

#: ../chkconfig.c:411
msgid "only one of --list, --add, or --del may be specified\n"
msgstr "solo una delle opzioni --list, --add o --del può essere specificata\n"

#: ../chkconfig.c:463
msgid "only one runlevel may be specified for a chkconfig query\n"
msgstr "solo un runlevel può essere specificato per una query chkconfig\n"

#: ../ntsysv.c:30
msgid "Press <F1> for more information on a service."
msgstr "Premere <F1> per maggiori informazioni sul servizio."

#: ../ntsysv.c:62 ../ntsysv.c:99
msgid "Ok"
msgstr "Ok"

#: ../ntsysv.c:62
msgid "Back"
msgstr "Indietro"

#: ../ntsysv.c:62
msgid "Cancel"
msgstr "Annulla"

#: ../ntsysv.c:74
msgid "What services should be automatically started?"
msgstr "Quale servizio avviare automaticamente?"

#: ../ntsysv.c:80
msgid "Services"
msgstr "Servizi"

