#: ../chkconfig.c:23
msgid ""
"This may be freely redistributed under the terms of the GNU Public License.\n"
msgstr ""
"Questo software puo' essere distribuito liberamente secondo la Licenza GNU "
"Public.\n"

#: ../chkconfig.c:26
#, c-format
msgid "usage:   %s --list [name]\n"
msgstr "usare:  %s --list [nome]\n"

#: ../chkconfig.c:27
#, c-format
msgid "         %s --add <name>\n"
msgstr "         %s --add <nome>\n"

#: ../chkconfig.c:28
#, c-format
msgid "         %s --del <name>\n"
msgstr "         %s --del <nome>\n"

#: ../chkconfig.c:29
#, c-format
msgid "         %s [--level <levels>] <name> %s)\n"
msgstr "         %s  [--level] <livelli>] <nome> %s)\n"

#: ../chkconfig.c:36
#, c-format
msgid "service %s does not support chkconfig\n"
msgstr "Il servizio %s non supporta chkconfig\n"

#: ../chkconfig.c:38
#, c-format
msgid "error reading information on service %s: %s\n"
msgstr "errore durante la lettura delle informazioni sul servizio %s: %s\n"

#: ../chkconfig.c:100
msgid "on"
msgstr "on"

#: ../chkconfig.c:100
msgid "off"
msgstr "off"

#: ../chkconfig.c:117
#, c-format
msgid "failed to open %s/init.d: %s\n"
msgstr "impossibile aprire %s/init.d: %s\n"

#: ../chkconfig.c:151
#, c-format
msgid "error reading from directory %s/init.d: %s"
msgstr "errore durante la lettura dalla directory %s/init.s: %s"

#: ../chkconfig.c:231
#, c-format
msgid "%s version %s\n"
msgstr "%s versione %s\n"

#: ../chkconfig.c:238
msgid "only one of --list, --add, or --del may be specified\n"
msgstr "solo una delle opzioni --list, --add o --del puo' essere specificata\n"

#: ../chkconfig.c:286
msgid "only one runlevel may be specified for a chkconfig query\n"
msgstr "solo un runlevel puo' essere specificato per una query chkconfig\n"

#: ../ntsysv.c:30
msgid "Press <F1> for more information on a service."
msgstr "Premere <F1> per maggiori informazioni sul servizio."

#: ../ntsysv.c:56 ../ntsysv.c:93
msgid "Ok"
msgstr "Ok"

#: ../ntsysv.c:56
msgid "Back"
msgstr "Indietro"

#: ../ntsysv.c:56
msgid "Cancel"
msgstr "Cancella"

#: ../ntsysv.c:68
msgid "What services should be automatically started?"
msgstr "Quale servizio dovrebbe essere automaticamente attivato?"

#: ../ntsysv.c:74
msgid "Services"
msgstr "Servizi"

#: ../ntsysv.c:164
#, c-format
msgid "error reading info for service %s: %s\n"
msgstr "errore durante la lettura di informazioni per il servizio %s: %s\n"

#: ../ntsysv.c:173
#, c-format
msgid "error reading from directory %s/init.d: %s\n"
msgstr "errore durante la lettura dalla directory %s/init.s: %s\n"

#: ../ntsysv.c:219
msgid "bad argument to --levels\n"
msgstr "argomento errato per il livello\n"

#: ../ntsysv.c:226
msgid "No services may be managed by ntsysv!\n"
msgstr "I servizi potrebbero non essere gestiti da ntsys!\n"

#: ../leveldb.c:265
#, c-format
msgid "failed to glob pattern %s: %s\n"
msgstr "impossibile inglobare il modello %s: %s\n"

#: ../leveldb.c:296
msgid "cannot determine current run level\n"
msgstr "non posso determinare l'attuale run-level\n"

#: ../leveldb.c:330
#, c-format
msgid "failed to make symlink %s: %s\n"
msgstr "impossibile creare il link simbolico %s: %s\n"
