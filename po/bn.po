msgid "common options: --verbose --test --help --usage --version\n"
msgstr "সাধারণ অপশন: --verbose --test --help --usage --version\n"

#: ../alternatives.c:58
msgid "                --altdir <directory> --admindir <directory>\n"
msgstr "                --altdir <directory> --admindir <directory>\n"

